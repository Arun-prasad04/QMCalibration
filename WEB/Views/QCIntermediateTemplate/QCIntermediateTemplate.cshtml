@{
    ViewData["Title"] = "Intermediate Template";
}
<script src="~/js/3.3.1Version/jquery.min.js"></script>
<style>
    table,
    th,
    tr,
    td {
        border-collapse: collapse;
        border: 1px solid black;
    }

    th{
        text-align: center;
    }

    td{
        text-align: left;
    }

    .Border0px {
        border: 0px;
        width: 100%;
    }

    .Tables-AndItsTextBox {
        width: 100%;
    }

    .SINo-Yes-No-NA_columns {
        width: 5%;
    }

    .Comments {
        width: 30%;
    }

    .CheckPoints {
        width: 50%;
    }

    .childTableTextBox {
        height: 20px;
        width: 100%;
    }

     .MasterEquipmentTextBox {
        height: 70px;
        width: 100%;
        border: 0px;
    }

    .wd100 {
        width: 100%;
    }

    .wd100ht40 {
        width: 100%;
        height: 40px;
    }

    .ht40 {
        height: 40px;
    }

    .ht80 {
        height: 80px;
    }

    .wd100ht80 {
        width: 100%;
        height: 80px;
    }

    .row {
        display: flex;

        margin-left: 2px;

        margin-right: 2px;
    }

    .column {

        flex: 50%;
    }

    .wd100ht35 {
        width: 100%;
        height: 32px;
        text-align: right;
    }

    .outline {
        outline: none;
        border: none;
    }

    .wd100textalign {
        width: 100%;
        text-align: right;
        outline: none;
        border: none;
    }
    .ReadOnly {
        background-color: #ADD8E6;
    }

    .statusalign{
        text-align: left;
    }
    .daimlerboxwidthInt{
        width: 180px;
    }
    .formatboxwidthInt{
        width: 50px;
        text-align: left;
    }
    .formatboxvaluewidthInt{
        width: 60px;
        text-align: left;
    }
    .revisiondatewidthInt{
        width: 20px;
        text-align: left;
    }

</style>

<form role="form" id="QCIntermediateForm" method="post" action='@Url.Action("SaveData", "QCIntermediateTemplate")'>
    <input type="hidden" name="Id" id="Id" value="@Model.Id">
    <input type="hidden" name="MasterEquipmentId2" id="MasterEquipmentId2" value="@Model.MasterEquipmentId2">
    <input type="hidden" id="DocumentStatus" name="DocumentStatus" value="@Model.DocumentStatus">
    <input type="hidden" id="StudyPerformedBy" name="StudyPerformedBy" value="@Model.StudyPerformedBy">
    <input type="hidden" id="TechnicalManager" name="TechnicalManager" value="@Model.TechnicalManager">
    <input type="hidden" id="DamageDent" name="DamageDent" value="@Model.DamageDent">
    <input type="hidden" id="RustAndOxidation" name="RustAndOxidation" value="@Model.RustAndOxidation">
    <input type="hidden" id="Accessories" name="Accessories" value="@Model.Accessories">
    <input type="hidden" id="PhysicalAbnormalities" name="PhysicalAbnormalities" value="@Model.PhysicalAbnormalities">
    <input type="hidden" id="PoweringIssue" name="PoweringIssue" value="@Model.PoweringIssue">
    <input type="hidden" id="SmoothMovement" name="SmoothMovement" value="@Model.SmoothMovement">
    <input type="hidden" id="NoiseObserved" name="NoiseObserved" value="@Model.NoiseObserved">
    <input type="hidden" id="SoftwareFaults" name="SoftwareFaults" value="@Model.SoftwareFaults">
    <input type="hidden" id="OtherAbnormalities" name="OtherAbnormalities" value="@Model.OtherAbnormalities">
    <input type="hidden" id="RevisionNo" name="RevisionNo" value="@Model.RevisionNo">
    <input type="hidden" id="RevisionDate" name="RevisionDate" value="@Model.RevisionDate">
    <input type="hidden" id="MasterEquipmentId1" name="MasterEquipmentId1"  value="@Model.MasterEquipmentId1">
    <input type="hidden" id="MasterEquipmentId2" name="MasterEquipmentId2" value="@Model.MasterEquipmentId2">
    <input type="hidden" id="MasterEquipmentId3" name="MasterEquipmentId3" value="@Model.MasterEquipmentId3">
    <input type="hidden" id="EquipmentName2" name="EquipmentName2" value="@Model.EquipmentName2">

    <table class="Tables-AndItsTextBox">
        <tr>
            <th rowspan="2" class="daimlerboxwidthInt headingtextalign">Daimler</th>
            <th rowspan="2" class="headingtextalign" style="500px">Intermediate Check Evaluation Mechanical Calibration</th>
            <td rowspan="2" >
                <div style="width: 100%;">
                    <table class="wd100">
                        <tr>
                            <th class="formatboxwidth">Format No.</th>
                            <td class="formatboxvaluewidth"><input type="text" value="@Model.FormatNo" id="FormatNo" name="FormatNo" class="wd100 outline ReadOnly"
                                    readonly></td>
                        </tr> 
                         <tr>
                            <th class="formatboxwidth">Rev.No & Date</th>
                            <td class="formatboxvaluewidth"><input type="text" value="@Model.RevisionNoAndDate" id="RevisionNoAndDate" name="RevisionNoAndDate"
                                    class="wd100 outline ReadOnly" readonly>
                            </td>
                        </tr>
                    </table>
                </div>
            </td>
        </tr>
    </table>
    <br>
    <table class="Tables-AndItsTextBox">
        <tr>
            <td rowspan="2" class="daimlerboxwidth">Device Name</td>
            <td rowspan="2"><input type="text" class="MasterEquipmentTextBox wd100 outline ReadOnly" value="@Model.EquipmentName1"
                    name="EquipmentName1" id="EquipmentName1" readonly>
            </td>
            <td>Master Equipment Id</td>
            <td colspan="2"><input type="text" name="Master1LabId" value="@Model.Master1LabId"
                    id="Master1LabId" class="wd100 outline"   onkeypress="updateMasterList()">
            </td>
        </tr>
        <tr>
            <td>Range /Size & Least Count</td>
            <td><input type="text" name="RangeOrSize1" value="@Model.RangeOrSize1" id="RangeOrSize1"
                    class="wd100 outline ReadOnly" readonly>
            </td>
            <td><input type="text" name="LeastCount1" value="@Model.LeastCount1" id="LeastCount1"
                    class="wd100 outline ReadOnly" readonly>
            </td>
        </tr>
        <tr>
            <td colspan="2">Master Equipment used to calibrate</td>
            <td colspan="4"><input type="text" name="Master2LabId" value="@Model.Master2LabId"
                    class="Border0px" id="Master2LabId" onkeypress="updateMasterList2()">
            </td>
        </tr>
    </table>
    <br>
    <table class="Tables-AndItsTextBox">
        <tr>
            <th class="SINo-Yes-No-NA_columns">Sl No.</th>
            <th class="CheckPoints">Check Points</th>
            <th class="SINo-Yes-No-NA_columns">Yes</th>
            <th class="SINo-Yes-No-NA_columns">No</th>
            <th class="SINo-Yes-No-NA_columns">NA</th>
            <th class="Comments">Comments</th>
        </tr>
        <tr>
            <th colspan="6">Physical Conditions</th>
        </tr>
        <tr>
            <td>1</td>
            <td>Any Damages, Dent & Scratches ?</td>
            <td>@if (@Model.DamageDent == "Y")
                {
                    <input type="radio" name="DamageDentYes" value="Y" checked="checked" id="DamageDentYes"
                    class="Tables-AndItsTextBox">
                }
                else
                {

                    <input type="radio" name="DamageDentYes" value="Y" id="DamageDentYes" class="Tables-AndItsTextBox">
                }
            </td>
            <td>@if (@Model.DamageDent == "N")
                {
                    <input type="radio" name="DamageDentNo" value="N" checked="checked" id="DamageDentNo"
                    class="Tables-AndItsTextBox">
                }
                else
                {

                    <input type="radio" name="DamageDentNo" value="N"  id="DamageDentNo" class="Tables-AndItsTextBox">
                }
            </td>
            <td>@if (@Model.DamageDent == "O")
                {
                    <input type="radio" name="DamageDentNA" value="O" checked="checked" id="DamageDentNA"
                    class="Tables-AndItsTextBox">
                }
                else
                {

                    <input type="radio" name="DamageDentNA" value="O" id="DamageDentNA" class="Tables-AndItsTextBox">
                }
            </td>
            <td><input type="text" value="@Model.CommentsOfDamageDent" class="Border0px" name="CommentsOfDamageDent"
                    id="CommentsOfDamageDent"></td>
        </tr>
        <tr>
            <td>2</td>
            <td>Rust/ Oxidation observed on the surfaces ?</td>
            <td>@if (@Model.RustAndOxidation == "Y")
                {
                    <input type="radio" name="RustAndOxidationYes" value="Y" checked="checked" id="RustAndOxidationYes"
                    class="Tables-AndItsTextBox">
                }
                else
                {

                    <input type="radio" name="RustAndOxidationYes" value="Y" id="RustAndOxidationYes"
                    class="Tables-AndItsTextBox">
                }
            </td>
            <td>@if (@Model.RustAndOxidation == "N")
                {
                    <input type="radio" name="RustAndOxidationNo" value="N" checked="checked" id="RustAndOxidationNo"
                    class="Tables-AndItsTextBox">
                }
                else
                {

                    <input type="radio" name="RustAndOxidationNo" value="N" id="RustAndOxidationNo"
                    class="Tables-AndItsTextBox">
                }
            </td>
            <td>@if (@Model.RustAndOxidation == "O")
                {
                    <input type="radio" name="RustAndOxidationNA" value="O" checked="checked" id="RustAndOxidationNA"
                    class="Tables-AndItsTextBox">
                }
                else
                {

                    <input type="radio" name="RustAndOxidationNA" value="O" id="RustAndOxidationNA"
                    class="Tables-AndItsTextBox">
                }
            </td>
            <td><input type="text" value="@Model.CommentsOfRustAndOxidation" class="Border0px"
                    id="CommentsOfRustAndOxidation" name="CommentsOfRustAndOxidation"></td>
        </tr>
        <tr>
            <td>3</td>
            <td>Accessories, Fixtures & any other attachments Present?</td>
            <td>@if (@Model.Accessories == "Y")
                {
                    <input type="radio" name="AccessoriesYes" value="Y" checked="checked" id="AccessoriesYes"
                    class="Tables-AndItsTextBox">
                }
                else
                {

                    <input type="radio" name="AccessoriesYes" value="Y" id="AccessoriesYes" class="Tables-AndItsTextBox">
                }
            </td>
            <td>@if (@Model.Accessories == "N")
                {
                    <input type="radio" name="AccessoriesNo" value="N" checked="checked" id="AccessoriesNo"
                    class="Tables-AndItsTextBox">
                }
                else
                {

                    <input type="radio" name="AccessoriesNo" value="N" id="AccessoriesNo" class="Tables-AndItsTextBox">
                }
            </td>
            <td>@if (@Model.Accessories == "O")
                {
                    <input type="radio" name="AccessoriesNA" value="O" checked="checked" id="AccessoriesNA"
                    class="Tables-AndItsTextBox">
                }
                else
                {

                    <input type="radio" name="AccessoriesNA" value="O" id="AccessoriesNA" class="Tables-AndItsTextBox">
                }
            </td>
            <td><input type="text" value="@Model.CommentsOfAccessories" class="Border0px" id="CommentsOfAccessories"
                    name="CommentsOfAccessories"></td>
        </tr>
        <tr>
            <td>4</td>
            <td>Any abnormalities found?</td>
            <td>@if (@Model.PhysicalAbnormalities == "Y")
                {
                    <input type="radio" name="PhysicalAbnormalitiesYes" value="Y" checked="checked" id="PhysicalAbnormalitiesYes"
                    class="Tables-AndItsTextBox">
                }
                else
                {

                    <input type="radio" name="PhysicalAbnormalitiesYes" value="Y" id="PhysicalAbnormalitiesYes"
                    class="Tables-AndItsTextBox">
                }
            </td>
            <td>@if (@Model.PhysicalAbnormalities == "N")
                {
                    <input type="radio" name="PhysicalAbnormalitiesNo" value="N" checked="checked" id="PhysicalAbnormalitiesNo"
                    class="Tables-AndItsTextBox">
                }
                else
                {

                    <input type="radio" name="PhysicalAbnormalitiesNo" value="N" id="PhysicalAbnormalitiesNo"
                    class="Tables-AndItsTextBox">
                }
            </td>
            <td>@if (@Model.PhysicalAbnormalities == "O")
                {
                    <input type="radio" name="PhysicalAbnormalitiesNA" value="O" checked="checked" id="PhysicalAbnormalitiesNA"
                    class="Tables-AndItsTextBox">
                }
                else
                {

                    <input type="radio" name="PhysicalAbnormalitiesNA" value="O" id="PhysicalAbnormalitiesNA"
                    class="Tables-AndItsTextBox">
                }
            </td>
            <td><input type="text" value="@Model.CommentsOfPhysicalAbnormalities" class="Border0px"
                    id="CommentsOfPhysicalAbnormalities" name="CommentsOfPhysicalAbnormalities">
            </td>
        </tr>
        <tr>
            <th colspan="6">Function Check</th>
        </tr>
        <tr>
            <td>1</td>
            <td>Any Powering issue?</td>
            <td>@if (@Model.PoweringIssue == "Y")
                {
                    <input type="radio" name="PoweringIssueYes" value="Y" checked="checked" id="PoweringIssueYes"
                    class="Tables-AndItsTextBox">
                }
                else
                {

                    <input type="radio" name="PoweringIssueYes" value="Y" id="PoweringIssueYes" class="Tables-AndItsTextBox">
                }
            </td>
            <td>@if (@Model.PoweringIssue == "N")
                {
                    <input type="radio" name="PoweringIssueNo" value="N" checked="checked" id="PoweringIssueNo"
                    class="Tables-AndItsTextBox">
                }
                else
                {

                    <input type="radio" name="PoweringIssueNo" value="N" id="PoweringIssueNo" class="Tables-AndItsTextBox">
                }
            </td>
            <td>@if (@Model.PoweringIssue == "O")
                {
                    <input type="radio" name="PoweringIssueNA" value="O" checked="checked" id="PoweringIssueNA"
                    class="Tables-AndItsTextBox">
                }
                else
                {

                    <input type="radio" name="PoweringIssueNA" value="O" id="PoweringIssueNA" class="Tables-AndItsTextBox">
                }
            </td>
            <td><input type="text" value="@Model.CommentsOfPoweringIssue" class="Border0px" id="CommentsOfPoweringIssue"
                    name="CommentsOfPoweringIssue"></td>
        </tr>
        <tr>
            <td>2</td>
            <td>Smooth movement for its entire range</td>
            <td>@if (@Model.SmoothMovement == "Y")
                {
                    <input type="radio" name="SmoothMovementYes" value="Y" checked="checked" id="SmoothMovementYes"
                    class="Tables-AndItsTextBox">
                }
                else
                {

                    <input type="radio" name="SmoothMovementYes" value="Y" id="SmoothMovementYes" class="Tables-AndItsTextBox">
                }
            </td>
            <td>@if (@Model.SmoothMovement == "N")
                {
                    <input type="radio" name="SmoothMovementNo" value="N" checked="checked" id="SmoothMovementNo"
                    class="Tables-AndItsTextBox">
                }
                else
                {

                    <input type="radio" name="SmoothMovementNo" value="N" id="SmoothMovementNo" class="Tables-AndItsTextBox">
                }
            </td>
            <td>@if (@Model.SmoothMovement == "O")
                {
                    <input type="radio" name="SmoothMovementNA" value="O" checked="checked" id="SmoothMovementNA"
                    class="Tables-AndItsTextBox">
                }
                else
                {

                    <input type="radio" name="SmoothMovementNA" value="O" id="SmoothMovementNA" class="Tables-AndItsTextBox">
                }
            </td>
            <td><input type="text" value="@Model.CommentsOfSmoothMovement" class="Border0px"
                    id="CommentsOfSmoothMovement" name="CommentsOfSmoothMovement"></td>
        </tr>
        <tr>
            <td>3</td>
            <td>Any noise observed while functioning of the equipment?</td>
            <td>@if (@Model.NoiseObserved == "Y")
                {
                    <input type="radio" name="NoiseObservedYes" value="Y" checked="checked" id="NoiseObservedYes"
                    class="Tables-AndItsTextBox">
                }
                else
                {

                    <input type="radio" name="NoiseObservedYes" value="Y" id="NoiseObservedYes" class="Tables-AndItsTextBox">
                }
            </td>
            <td>@if (@Model.NoiseObserved == "N")
                {
                    <input type="radio" name="NoiseObservedNo" value="N" checked="checked" id="NoiseObservedNo"
                    class="Tables-AndItsTextBox">
                }
                else
                {

                    <input type="radio" name="NoiseObservedNo" value="N" id="NoiseObservedNo" class="Tables-AndItsTextBox">
                }
            </td>
            <td>@if (@Model.NoiseObserved == "O")
                {
                    <input type="radio" name="NoiseObservedNA" value="O" checked="checked" id="NoiseObservedNA"
                    class="Tables-AndItsTextBox">
                }
                else
                {

                    <input type="radio" name="NoiseObservedNA" value="O" id="NoiseObservedNA" class="Tables-AndItsTextBox">
                }
            </td>
            <td><input type="text" value="@Model.CommentsOfNoiseObserved" class="Border0px" id="CommentsOfNoiseObserved"
                    name="CommentsOfNoiseObserved"></td>
        </tr>
        <tr>
            <td>4</td>
            <td>Any faults with functioning of software?</td>
            <td>@if (@Model.SoftwareFaults == "Y")
                {
                    <input type="radio" name="SoftwareFaultsYes" value="Y" checked="checked" id="SoftwareFaultsYes"
                    class="Tables-AndItsTextBox">
                }
                else
                {

                    <input type="radio" name="SoftwareFaultsYes" value="Y" id="SoftwareFaultsYes" class="Tables-AndItsTextBox">
                }
            </td>
            <td>@if (@Model.SoftwareFaults == "N")
                {
                    <input type="radio" name="SoftwareFaultsNo" value="N" checked="checked" id="SoftwareFaultsNo"
                    class="Tables-AndItsTextBox">
                }
                else
                {

                    <input type="radio" name="SoftwareFaultsNo" value="N" id="SoftwareFaultsNo" class="Tables-AndItsTextBox">
                }
            </td>
            <td>@if (@Model.SoftwareFaults == "O")
                {
                    <input type="radio" name="SoftwareFaultsNA" value="O" checked="checked" id="SoftwareFaultsNA"
                    class="Tables-AndItsTextBox">
                }
                else
                {

                    <input type="radio" name="SoftwareFaultsNA" value="O" id="SoftwareFaultsNA" class="Tables-AndItsTextBox">
                }
            </td>
            <td><input type="text" value="@Model.CommentsOfSoftwareFaults" class="Border0px"
                    id="CommentsOfSoftwareFaults" name="CommentsOfSoftwareFaults">
            </td>
        </tr>
        <tr>
            <td>5</td>
            <td>Any other abnormalities ?</td>
            <td>@if (@Model.OtherAbnormalities == "Y")
                {
                    <input type="radio" name="OtherAbnormalitiesYes" value="Y" checked="checked" id="OtherAbnormalitiesYes"
                    class="Tables-AndItsTextBox">
                }
                else
                {

                    <input type="radio" name="OtherAbnormalitiesYes" value="Y" id="OtherAbnormalitiesYes"
                    class="Tables-AndItsTextBox">
                }
            </td>
            <td>@if (@Model.OtherAbnormalities == "N")
                {
                    <input type="radio" name="OtherAbnormalitiesNo" value="N" checked="checked" id="OtherAbnormalities"
                    class="Tables-AndItsTextBox">
                }
                else
                {

                    <input type="radio" name="OtherAbnormalitiesNo" value="N" id="OtherAbnormalitiesNo"
                    class="Tables-AndItsTextBox">
                }
            </td>
            <td>@if (@Model.OtherAbnormalities == "O")
                {
                    <input type="radio" name="OtherAbnormalitiesNA" value="O" checked="checked" id="OtherAbnormalitiesNA"
                    class="Tables-AndItsTextBox">
                }
                else
                {

                    <input type="radio" name="OtherAbnormalitiesNA" value="O" id="OtherAbnormalitiesNA"
                    class="Tables-AndItsTextBox">
                }
            </td>
            <td><input type="text" value="@Model.CommentsOfOtherAbnormalities" class="Border0px"
                    id="CommentsOfOtherAbnormalities" name="CommentsOfOtherAbnormalities"></td>
        </tr>
    </table>
    <br>
    <table class="Tables-AndItsTextBox">
        <tr>
            <th colspan="6">Accuracy Check</th>
        </tr>
        <tr>
            <td colspan="3">Equipment used to Check the Master Equipment</td>

            <td><input type="text" name="EquipmentName3" value="@Model.EquipmentName3" class="wd100 outline ReadOnly" readonly
                    id="EquipmentName3" ></td>
            <td>Equipment Id</td>
            <td><input type="text" name="Master3LabId" value="@Model.Master3LabId" id="Master3LabId"
                    class="wd100 outline" onkeypress="updateMasterList3()" >
            </td>

        </tr>
        <tr>
            <td colspan="3">Range / Size</td>
            <td><input type="text" name="RangeOrSize3" value="@Model.RangeOrSize3" id="RangeOrSize3"
                    class="wd100 outline ReadOnly" readonly>
            </td>
            <td>Least Count</td>
            <td><input type="text" name="LeastCount3" value="@Model.LeastCount3" id="LeastCount3"
                    class="wd100 outline ReadOnly" readonly>
            </td>
        </tr>

    </table>
    <br>
    @if (@Model.DocumentStatus == 0)
    {
        <p align="right">
            <button style="align-content: right: ;;" id="add" name="add" type="button" class="btn btn-primary">Add New Row</button>
        </p>
    }
    <div class="Tables-AndItsTextBox">
        <table id="intermediateChildTable" class="Tables-AndItsTextBox">       
                <thead>
                    <tr>
                        <td colspan="4"></td>
                        <td>All values are in</td>
                        <td><input type="text" name="DataUnit" value="@Model.DataUnit" id="DataUnit">
                        </td>
                    </tr>
                <tr>
                    <td style="word-break: break-all;">Master Equipment <br>
                        Calibration Results</td>
                    <td style="word-break: break-all;">Current Internal <br>
                             Check</td>
                    <td style="word-break: break-all;">Difference</td>
                    <td style="word-break: break-all;">Acceptance Criteria</td>
                    <td style="word-break: break-all;">Decision</td>
                     @if (@Model.DocumentStatus == 0)
                     {
                        <td style="word-break: break-all;">Delete</td>
                     }
                </tr>
                </thead>
               <tbody>
                @foreach (var item in Model.QCIntermediateResultViewModelList)
                {
                    <tr id="tablerow0">
                        <td>
                            <input name="CalibrationResult" id="CalibrationResult" type="text"
                            value="@item.CalibrationResult" onkeyup="GetValue('0');" required="required" style="width: 100%;" />
                        </td>
                        <td>
                            <input name="CurrentInternalCheck" id="CurrentInternalCheck" type="text"
                            value="@item.CurrentInternalCheck" onkeyup="GetValue('0');" required="required" style="width: 100%;"  />
                        </td>
                        <td>
                            <input name="DifferenceResult" id="DifferenceResult" type="text" value="@item.DifferenceResult"
                            required="required" style="width: 100%;" readonly />
                        </td>
                        <td>
                            <input name="AcceptanceCriteria" id="AcceptanceCriteria" type="text"
                            value="@item.AcceptanceCriteria" required="required" style="width: 100%;" />
                        </td>
                        <td>
                            <input name="Decision" id="Decision" type="text" value="@item.Decision" required="required" style="width: 100%;" />
                        </td>

                        @if (@Model.DocumentStatus == 0)
                        {
                            <td>
                                <i class="fa fa-fw fa-trash" onclick="removeTr(0);"></i>
                            </td>
                        }
                    </tr>
                }
        </tbody>
        </table>
    </div>
   
    <table class="Tables-AndItsTextBox">
        <tr>
            <th colspan="6">Conclusions & Actions</th>
        </tr>
        <tr>
            <td colspan="2">Any doubts on Performance / Accuracy</td>
            <td colspan="4" ><input type="text" name="DoubtsOfPerformance" class="Border0px" value="@Model.DoubtsOfPerformance"
                    id="DoubtsOfPerformance"></td>
        </tr>
        <tr>
            <td colspan="2">Comments on Acceptance</td>
            <td  colspan="4"><input type="text" name="CommentsOfAcceptance" class="Border0px" value="@Model.CommentsOfAcceptance"
                    id="CommentsOfAcceptance"></td>
        </tr>
        <tr>
            <td colspan="2">Any other comments</td>
            <td  colspan="4"><input type="text" name="OtherComments" class="Border0px" value="@Model.OtherComments"
                    id="OtherComments"></td>
        </tr>
        <tr>     
            @if (@Model.DocumentStatus >= 1)
            {                   
                <td colspan="2">Study Performed by</td>
                <td><input type="text" name="StudyPerformedByName" value="@Model.StudyPerformedByName"
                        id="StudyPerformedByName" class="wd100 outline ReadOnly" readonly>
                </td>
                    <td colspan="2">Technical Manager</td>
                    <td><input type="text" name="TechnicalManagerName" value="@Model.TechnicalManagerName"
                        id="TechnicalManagerName" class="wd100 outline ReadOnly" readonly>
                    </td>
            } 
            else
            {
                <td colspan="2">Study Performed by</td>
