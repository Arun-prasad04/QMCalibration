<script>

    var counter = 1;
    $(function () {
        $('#add').click(function () {
            $('<tr id="tablerow' + counter + '"><td>' +
                '<input type="text" class="childTableTextBox" id="item.CalibrationResult"  name="item.CalibrationResult" onkeyup="GetValue('+counter+');" value="" required="required" />' +
                '</td>' +
                '<td>' +
                '<input type="text" class="childTableTextBox" id="item.CurrentInternalCheck"  name="item.CurrentInternalCheck" onkeyup="GetValue('+counter+');" value="" required="required" />' +
                '</td>' +
                '<td>' +
                '<input type="text" class="childTableTextBox" id="item.DifferenceResult" name="item.DifferenceResult" value="" required="required" />' +
                '</td>' +
                '<td>' +
                '<input type="text" class="childTableTextBox" id="item.AcceptanceCriteria" name="item.AcceptanceCriteria" value="" required="required" />' +
                '</td>' +
                '<td>' +
                '<input type="text" class="childTableTextBox" id="item.Decision" name="item.Decision" value="" required="required" />' +
                '</td>' +
                '<td>' +
                '<i class="fa fa-fw fa-trash" onclick="removeTr(' + counter + ');"></i>' +
                '</td>' +
                '</tr>').appendTo('#intermediateChildTable');
            counter++;
            return false;
        });
    });

    function removeTr(index) {
        if (counter > 1) {
            $('#tablerow' + index).remove();
            counter--;
        }
        return false;
    }
    function GetValue(Id)
    {
        $('#tablerow'+Id+'').each(function() {
            debugger;
            var col1=$(this).find("td:eq(0) input[type='text']").val(); // get current row 1st TD value
            var col2=$(this).find("td:eq(1) input[type='text']").val(); // get current row 2nd TD
            var col3=$(this).find("td:eq(2) input[type='text']").val(); // get current row 3rd TD 
            var Total=col1-col2;
            $(this).find("td:eq(2) input[type='text']").val(Total);              
        });
    }
</script>


<script type="text/javascript">
