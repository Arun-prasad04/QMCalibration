@using Microsoft.AspNetCore.Http
@inject Microsoft.AspNetCore.Http.IHttpContextAccessor HttpContextAccessor
<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8"></meta>
    <meta http-equiv="X-UA-Compatible" content="IE=edge"></meta>
    <meta name="viewport" content="width=device-width, initial-scale=1.0"></meta>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8"></meta>
    <title>Document</title>
</head>

<body style="font-family: Calibri;">
    <!-- page1 start-->
    <div style="padding-left: 5%; padding-right: 5%; margin-bottom: 5%;">
        
        <table style="border-collapse: collapse; border:2px solid #000">
            <tbody>
                <tr>
                    <td style="border-collapse: collapse; border:2px solid #000; text-align: center; width:250px">
                        @if(Model.IsNABL ?? false)
                        {
                            <img src="../image/NABL.png" style="padding-top:5px;" width="100px"  />
                        }
                        else
                        {
                            <img src="../image/Bharat_Benz.png" style="padding-top:5px;" width="150px" />
                        }
                    </td>
                    <td style="border-collapse: collapse; border:2px solid #000; text-align: center; width:840px">
                        <div>
                            <h5 class="trn" style="margin: 5px;">Daimler India Commercial Vehicles Pvt. Ltd -Central Lab</h5>
                            <p class="trn" style="margin: 5px; font-size: 12px;">SIPCOT Insdustrial Estate, Oragadam Village,Singaperumal Koil Road,</p>
                            <p class="trn" style="margin: 5px; font-size: 12px;">Mathur(PO), Sriperumbadur Taluk, Kanchipuram -602105, Tamil Nadu, India.</p>
                            <p class="trn" style="margin: 5px; font-size: 12px;">Phone: +91 44 4969 4456,</p>
                            <p class="trn" style="margin: 5px; font-size: 12px;">Website: www.daimler-indiacv.com / .bharatbenz.com</p>
                        </div>
                    </td>
                    <td style="border-collapse: collapse; border:2px solid #000; text-align: center; width:100px">
                        <img style="padding-left:40px;padding-top:5px;" id="imgQRCode" height="150px" width="190px" src="@ViewBag.QRCodeImage" />
                    </td>
                </tr>
            </tbody>
        </table>
        <div>
            <h2 class="trn" style="text-align: center; text-transform: uppercase;">Certificate of calibration</h2>
        </div>
        <div style="border-collapse: collapse; border:2px solid #000">
            <div>
                <div style="padding: 10px;">
                    <div>
                        <p  class="trn"  style="width:325px; font-weight: 900;padding-bottom: 25px; width: 600px;">I. Descriptions of DUC</p>
                    </div>
                    <table>
                        <tbody>
                            <tr>
                                <td class="trn"  style="width:325px; font-weight: 900;padding-bottom: 25px;">Certificate No</td>
                                <td style="width:400px;padding-bottom: 25px;">: @Model.LeverDialData.CertificateFormat</td>
                                <td class="trn" style="width:340px; font-weight: 900;padding-bottom: 25px;">Report Issues Date</td>
                                <td style="width:400px;padding-bottom: 25px;">: @DateTime.Now.ToString("dd-MM-yyyy")</td>
                            </tr>
                            @if(Model.IsNABL ?? false)
                            {
                                <tr>
                                    <td class="trn"  style="width:325px; font-weight: 900;padding-bottom: 25px;">ULR No</td>
                                    <td style="width:400px;padding-bottom: 25px;">: @Model.LeverDialData.ULRFormat</td>
                                    <td style="width:325px; font-weight: 900;padding-bottom: 25px;"></td>
                                    <td style="width:400px;padding-bottom: 25px;"></td>
                                </tr>
                            }
                            <tr>
                                <td class="trn"  style="width:325px; font-weight: 900; padding-bottom: 25px;">Date of Calibration</td>
                                <td style="width:400px; padding-bottom: 25px;">:@Model.LeverDialData.CalibrationPerformedDate.ToString("dd-MM-yyyy")</td>
                                <td class="trn"  style="width:325px; font-weight: 900; padding-bottom: 25px;">Next Calibration Due</td>
                                @if(Model.Result == "Rejected")
                                    {
                                        <td style="width:400px; padding-bottom: 25px;">: --</td>
                                    }
                                    else
                                    {
                                <td style="width:400px; padding-bottom: 25px;">: @Model.DueDate.ToString("MMM-yyyy")</td>
                                    }
                            </tr>
                            <tr>
                                <td class="trn" style="width:325px; font-weight: 900; padding-bottom: 25px;">Name of the Customer</td>
                                <td style="width:400px; padding-bottom: 25px;">: @Model.DepartmentName</td>
                                <td style="width:325px; font-weight: 900;padding-bottom: 25px;"></td>
                                <td style="width:400px;padding-bottom: 25px;"></td>
                            </tr>
                            <tr>
                                <td class="trn" style="width:325px; font-weight: 900;padding-bottom: 25px;">Department</td>
                                <td style="width:400px;padding-bottom: 25px;">: @Model.DepartmentName</td>
                                <td style="width:325px; font-weight: 900; padding-bottom: 25px;"></td>
                                <td style="width:400px; padding-bottom: 25px;"></td>
                            </tr>
                            <tr>
                                <td class="trn" style="width:325px; font-weight: 900; padding-bottom: 25px;">Date of Receipt</td>
                                <td style="width:400px; padding-bottom: 25px;">: @Model.DateOfReceipt.ToString("dd-MM-yyyy")</td>
                                <td style="width:325px; font-weight: 900; padding-bottom: 25px;"></td>
                                <td style="width:400px; padding-bottom: 25px;"></td>
                            </tr>
                            <tr>
                                <td class="trn" style="width:325px; font-weight: 900; padding-bottom: 25px;">Description of Equipment</td>
                                <td style="width:400px; padding-bottom: 25px;">: @Model.InstrumentName</td>
                                <td style="width:325px; font-weight: 900; padding-bottom: 25px;"></td>
                                <td style="width:400px; padding-bottom: 25px;"></td>
                            </tr>
                        </tbody>
                    </table>


                    <div style="padding-top: 10px;">
                        <table style="border-collapse: collapse; border:1px solid #000">
                            <tr>
                                <th class="trn" style="border-collapse: collapse; border:1px solid #000; text-align: center; width:825px; padding: 5px;">Least Count</th>
                                <th class="trn" style="border-collapse: collapse; border:1px solid #000; text-align: center; width:825px; padding: 5px;">Serial No</th>
                                <th class="trn" style="border-collapse: collapse; border:1px solid #000; text-align: center; width:825px; padding: 5px;">Make</th>
                            </tr>
                            <tr>
                                <td style="border-collapse: collapse; border:1px solid #000; text-align: center; width:825px">
                                    <p>@Model.Range</p>
                                </td>
                                <td style="border-collapse: collapse; border:1px solid #000; text-align: center; width:825px">
                                    <p>@Model.LC</p>
                                </td>
                                <td style="border-collapse: collapse; border:1px solid #000; text-align: center; width:825px">
                                    <p>@Model.SlNo</p>
                                </td>
                                <td style="border-collapse: collapse; border:1px solid #000; text-align: center; width:825px">
                                    <p>@Model.Make</p>
                                </td>
                            </tr>
                        </table>

                        <table style="margin-top:15px">
                            <tbody>
                                <tr>
                                    <td class="trn" style="width:325px; font-weight: 900;padding-bottom: 25px;">Identification Number</td>
                                    <td style="width:400px;padding-bottom: 25px;">: @Model.IdNo</td>
                                    <td style="width:325px; font-weight: 900;padding-bottom: 25px;"></td>
                                    <td style="width:400px;padding-bottom: 25px;"></td>
                                </tr>
                                <tr>
                                    <td class="trn" style="width:325px; font-weight: 900;padding-bottom: 25px;">Condition of DUC</td>
                                    <td style="width:400px;padding-bottom: 25px;">: @Model.LeverDialData.DialIndicatiorCondition</td>
                                    <td style="width:325px; font-weight: 900;padding-bottom: 25px;"></td>
                                    <td style="width:400px;padding-bottom: 25px;"></td>
                                </tr>
                                <tr>
                                    <td class="trn" style="width:325px; font-weight: 900; padding-bottom: 25px;">Calibration Procedure  /  Reference Standard :</td>
                                    <td style="width:400px; padding-bottom: 25px;">: @Model.StandardReffered</td>
                                    <td style="width:325px; font-weight: 900; padding-bottom: 25px;"></td>
                                    <td style="width:400px; padding-bottom: 25px;"></td>
                                </tr>
                            </tbody>
                        </table>
                    </div>

                    <div>
                        <p class="trn" style="width:325px; font-weight: 900;padding-bottom: 25px; width: 600px;">II. Master & Equipment Used For Calibration & Traceability</p>
                    </div>
                    <div style="margin-top: 10px;">
                        <table style="border-collapse: collapse; border:1px solid #000">
                            <tr>
                                <th class="trn" style="border-collapse: collapse; border:1px solid #000; text-align: center; width:325px">SI.</th>
                                <th class="trn" style="border-collapse: collapse; border:1px solid #000; text-align: center; width:325px">Master Equipment</th>
                                <th class="trn" style="border-collapse: collapse; border:1px solid #000; text-align: center; width:325px">Id No</th>
                                <th class="trn" style="border-collapse: collapse; border:1px solid #000; text-align: center; width:325px">Make </th>
                                <th  class="trn" style="border-collapse: collapse; border:1px solid #000; text-align: center; width:325px">Traceability</th>
                                <th  class="trn" style="border-collapse: collapse; border:1px solid #000; text-align: center; width:325px">Certificate No</th>
                                <th class="trn" style="border-collapse: collapse; border:1px solid #000; text-align: center; width:325px">Validity </th>
                            </tr>
                            @{ int i=1;
                            @foreach (var item in @Model.MasterEqiupmentList)
                            {
                            <tr>
                                <td style="border-collapse: collapse; border:1px solid #000; text-align: center; width:325px">@i</td>
                                <td style="border-collapse: collapse; border:1px solid #000; text-align: center; width:325px">@item.EquipName</td>
                                <td style="border-collapse: collapse; border:1px solid #000; text-align: center; width:325px">@item.LabId</td>
                                <td style="border-collapse: collapse; border:1px solid #000; text-align: center; width:325px">@item.Make</td>
                                <td style="border-collapse: collapse; border:1px solid #000; text-align: center; width:325px">@item.Traceability</td>
                                <td style="border-collapse: collapse; border:1px solid #000; text-align: center; width:325px">@item.CertNo</td>
                                <td style="border-collapse: collapse; border:1px solid #000; text-align: center; width:325px">@item.DueDate.ToString("dd-MM-yyyy")</td>
                            </tr>
                            i++;
                            }
                           }
                        </table>
                        <p class="fw-normal fontsize14 trn">The master Equipment used for calibration are calibrated and traceable to National / International Standard</p>
                    </div>

                    <table style="margin-top:15px">
                        <tbody>
                            <tr>
                                <td class="trn" style="width:325px; font-weight: 900;padding-bottom: 25px;">Environmental Condition</td>
                                <td style="width:400px;padding-bottom: 10px;"><p>20 &plusmn; 1.0<sup>o</sup>C / 50 &plusmn; 10% RH</p></td>
                                <td style="width:325px; font-weight: 900;padding-bottom: 25px;"></td>
                                <td style="width:400px;padding-bottom: 25px;"></td>
                            </tr>
                            <tr>
                                <td class="trn" style="width:325px; font-weight: 900;padding-bottom: 25px;">Uncertainty Of Measurement</td>
                                <td style="width:400px;padding-bottom: 25px;"><input class="form-control" placeholder="Uncertainty Of Measurement" id="Uncertainity" name="Uncertainity" value="@Model.LeverDialData.Uncertainity"></td>
                                <td style="width:325px; font-weight: 900;padding-bottom: 25px;"></td>
                                <td style="width:400px;padding-bottom: 25px;"></td>
                            </tr>
                        </tbody>
                    </table>

                    <p class="trn">The stated Expanded uncertainty is estimatd at a confidence level approximately 95% with a coverage factor k = 2</p>

                    <table style="margin-top:15px">
                        <tbody>
                            <tr>
                                <td style="width:325px; padding-bottom: 25px; width: 800px;">
                                    <div class="col-6 text-start">
                                        <h5 class="trn" style="margin-bottom: 5px;">Calibrated By</h5>
                                        <img src="../UserSignature/@Model.LeverDialData.PerformedBySign" style="width: 100px;"/>
                                        <p style="margin: 0px;">@Model.LeverDialData.CalibrationPerformedBy</p>
                                        <p style="margin: 0px;">@Model.LeverDialData.PerformedByDesignation</p>
                                    </div>
                                </td>
                                <td style="width:400px;padding-bottom: 25px; width: 800px; text-align: right;">
                                    <h5 class="trn" style="margin-bottom: 5px;">Authorized Signatory</h5>
                                    <img src="../UserSignature/@Model.LeverDialData.ReviewedBySign" style="width: 100px;"/>
                                    <p style="margin: 0px;">@Model.LeverDialData.ReviewedBy</p>
                                    <p style="margin: 0px;">@Model.LeverDialData.ReviewedByDesignation</p>
                                </td>
                            </tr>
                        </tbody>
                    </table>

                </div>
                <div style="border-collapse: collapse; 
                    border-top: solid;
                    border-block-width: 2px; border: 2px solid #000; display: flex;">
                    <div style="padding:10px">
                        <p  class="trn" style="margin: 0px; font-size: 12px;">1. The Calibration results are valid at the time of & under the stated conditions of measurement.</p>
                        <p  class="trn" style="margin: 0px; font-size: 12px;">2. The report shoult not be reproduced either in full or in part without out permission in writing.</p>
                        <p class="trn" style="margin: 0px; font-size: 12px;">3. Next calibration due date given in this cetificate as per customer request irrespective of the status or condition of the IMTE.</p>
                    </div>
                    <div><img style="padding:10px;" src="../image/footer1.png" alt=""  height="60px" width="250px"></div>
                </div>
            </div>
             
        </div>
        <table style="margin-top:15px">
                <tbody>
                    <tr style="font-size: xx-small;">
                        <td style="width:325px; padding-bottom: 25px; width: 800px;">
                            <div>
                            <h4 class="trn" style="font-size: small;">Format No : 365.QM.D.16, Rev No : 02</h4>
                            </div>
                        </td>
                        <td style="width:400px;padding-bottom: 25px; width: 800px; text-align: right;">
                        <h4 class="trn" style="font-size: small;">Page 1 of 2</h4>
                        </td>
                    </tr>
                </tbody>
            </table>
    </div>
    
    <!-- page1 end-->
    <!-- page2 start-->
    <div style="padding-left: 5%; padding-right: 5%; margin-bottom: 5%;">
        <table style="border-collapse: collapse; border:2px solid #000">
            <tbody>
                <tr>
                    <td style="border-collapse: collapse; border:2px solid #000; text-align: center; width:250px">
                       @if(Model.IsNABL ?? false)
                        {
                            <img src="../image/NABL.png" style="padding-top:5px;" width="100px" />
                        }
                        else
                        {
                            <img src="../image/Bharat_Benz.png" style="padding-top:5px;" width="150px" />
                        }
                    </td>
                    <td style="border-collapse: collapse; border:2px solid #000; text-align: center; width:840px">
                        <div>
                            <h5 class="trn" style="margin: 5px;">Daimler India Commercial Vehicles Pvt. Ltd -Central Lab</h5>
                            <p class="trn" style="margin: 5px; font-size: 12px;">SIPCOT Insdustrial Estate, Oragadam Village,Singaperumal Koil Road,</p>
                            <p class="trn" style="margin: 5px; font-size: 12px;">Mathur(PO), Sriperumbadur Taluk, Kanchipuram -602105, Tamil Nadu, India.</p>
                            <p class="trn" style="margin: 5px; font-size: 12px;">Phone : +91 44 4969 4456,</p>
                            <p class="trn" style="margin: 5px; font-size: 12px;">Website: www.daimler-indiacv.com / .bharatbenz.com.</p>
                        </div>
                    </td>
                    <td style="border-collapse: collapse; border:2px solid #000; text-align: center; width:100px">
                        <img style="padding-left:40px;padding-top:5px;" id="imgQRCode" height="150px" width="190px" src="@ViewBag.QRCodeImage" />
                    </td>
                </tr>
            </tbody>
        </table>
        <div style="border: 2px solid #000; padding: 10px;">
            <table style="margin-top:30px">
                <tbody>
                    <tr>
                        <td class="trn" style="font-weight: 900;padding-bottom: 25px; width: 600px;">III. Mechanical Calibration Results</td>
                        <td class="trn" style="padding-bottom: 25px;font-weight: 900; width: 600px; text-align: center;">Certificate No</td>
                        <td style="width:400px;padding-bottom: 25px;">: @Model.LeverDialData.CertificateFormat</td>
                        <td class="trn" style="width:325px; font-weight: 900;padding-bottom: 25px; width: 600px; text-align: center;">Report Issue Date :</td>
                        <td style="width:325px;padding-bottom: 25px;">@DateTime.Now.ToString("dd-MM-yyyy")</td>
                    </tr>
                </tbody>
            </table>

            <div class="p-3">
                <p style="text-align: center;">All Values are in @Model.LeverDialData.Allvalues </p>
                <table style="border-collapse: collapse; border:1px solid #000">
                    <tr>
                        <th style="border-collapse: collapse; border:1px solid #000; text-align: center; width:50px; padding:5px" rowspan="2" class="text-center trn trn">Sl No.</th>
                        <th style="border-collapse: collapse; border:1px solid #000; text-align: center; width:325px; padding:5px" rowspan="2" colspan="2" class="text-center trn">Parameter</th>
                        <th style="border-collapse: collapse; border:1px solid #000; text-align: center; width:325px; padding:5px" rowspan="2" class="text-center trn">Specification</th>
                        <th style="border-collapse: collapse; border:1px solid #000; text-align: center; width:325px; padding:5px" colspan="4" class="text-center trn">Actual</th>
                        
                    </tr>
                    <tr>
                        <th style="border-collapse: collapse; border:1px solid #000; text-align: center; width:325px; padding:5px" colspan="2" class="text-center trn">Direction A</th>
                        <th style="border-collapse: collapse; border:1px solid #000; text-align: center; width:325px; padding:5px" colspan="2" class="text-center trn">Direction B</th>  
                    </tr>
                    <tr>
                        <td style="border-collapse: collapse; border:0.5px solid #000; text-align: center; width:50px; padding:5px">1</td>
                        <td class="trn" style="border-collapse: collapse; border:0.5px solid #000; text-align: center; width:50px; padding:5px;writing-mode: vertical-rl;transform: rotate(180deg);" rowspan="4">Indication Error</td>
                            <td class="trn" style="border-collapse: collapse; border:0.5px solid #000; text-align: center; width:325px; padding:5px">Measuring Range</td>
                            <td style="border-collapse: collapse; border:0.5px solid #000; text-align: center; width:325px; padding:5px"
                                >@Model.LeverDialData.MeasuringRangeSpec</td>
                            <td
                                style="border-collapse: collapse; border:0.5px solid #000; text-align: center; width:325px; padding:5px" colspan="2">
                                @Model.LeverDialData.MeasuringRangeDirectionA1</td>
                            <td
                                style="border-collapse: collapse; border:0.5px solid #000; text-align: center; width:325px; padding:5px" colspan="2">
                                @Model.LeverDialData.MeasuringRangeDirectionB1</td>
                        </tr>
                        <tr>
                        <td class="trn" style="border-collapse: collapse; border:0.5px solid #000; text-align: center; width:50px; padding:5px">2</td>
                        <td class="trn" style="border-collapse: collapse; border:0.5px solid #000; text-align: center; width:325px; padding:5px">10 scale Division</td>
                            <td style="border-collapse: collapse; border:0.5px solid #000; text-align: center; width:325px; padding:5px"
                                >@Model.LeverDialData.ScaleDivisionSpec</td>
                            <td
                                style="border-collapse: collapse; border:0.5px solid #000; text-align: center; width:325px; padding:5px" colspan="2">
                                @Model.LeverDialData.ScaleDivisionDirectionA2</td>
                            <td
                                style="border-collapse: collapse; border:0.5px solid #000; text-align: center; width:325px; padding:5px" colspan="2">
                                @Model.LeverDialData.ScaleDivisionDirectionB2</td>
                        </tr>
                        <tr>
                            <td style="border-collapse: collapse; border:0.5px solid #000; text-align: center; width:50px; padding:5px">3</td>
                            <td class="trn" style="border-collapse: collapse; border:0.5px solid #000; text-align: center; width:325px; padding:5px"
                                >Hysteresis</td>
                            <td style="border-collapse: collapse; border:0.5px solid #000; text-align: center; width:325px; padding:5px"
                                >@Model.LeverDialData.HysteresisSpec</td>
                            <td style="border-collapse: collapse; border:0.5px solid #000; text-align: center; width:325px; padding:5px"
                                colspan="2">@Model.LeverDialData.HysteresisDirectionA3</td>
                            <td style="border-collapse: collapse; border:0.5px solid #000; text-align: center; width:325px; padding:5px"
                                colspan="2">@Model.LeverDialData.HysteresisDirectionB3</td>
                        </tr>
                        <tr>
                            <td style="border-collapse: collapse; border:0.5px solid #000; text-align: center; width:50px; padding:5px">4</td>
                            <td class="trn" style="border-collapse: collapse; border:0.5px solid #000; text-align: center; width:325px; padding:5px"
                                >Repeatability</td>
                            <td style="border-collapse: collapse; border:0.5px solid #000; text-align: center; width:325px; padding:5px"
                                >@Model.LeverDialData.RepeatabilitySpec</td>
                            <td style="border-collapse: collapse; border:0.5px solid #000; text-align: center; width:325px; padding:5px"
                                colspan="2">@Model.LeverDialData.RepeatabilityDirectionA4</td>
                            <td style="border-collapse: collapse; border:0.5px solid #000; text-align: center; width:325px; padding:5px"
                                colspan="2">@Model.LeverDialData.RepeatabilityDirectionB4</td>
                        </tr>
                </table>

                <table style="margin-top:15px">
                    <tbody>
                        <tr>
                            <td class="trn" style="width:325px; font-weight: 900;padding-bottom: 25px;">IV. Result Of Calibration :</td>
                            <td style="width:400px;padding-bottom: 25px;"><input class="form-control" placeholder="Result Of Calibration" id="CalibrationResult" name="CalibrationResult" value="@Model.LeverDialData.CalibrationResult"></td>
                            <td style="width:325px; font-weight: 900;padding-bottom: 25px;"></td>
                            <td style="width:400px;padding-bottom: 25px;"></td>
                        </tr>
                        <tr>
                            <td class="trn" style="width:325px; font-weight: 900;padding-bottom: 25px;">V. Remarks :</td>
                            <td style="width:400px;padding-bottom: 25px;"><input class="form-control" placeholder="Remarks" id="Remarks" name="Remarks" value="@Model.LeverDialData.Remarks"></td>
                            <td style="width:325px; font-weight: 900;padding-bottom: 25px;"></td>
                            <td style="width:400px;padding-bottom: 25px;"></td>
                        </tr>
                         <tr>
                            <td style="width:325px; font-weight: 900;padding-bottom: 25px;"></td>
                            <td  colspan="3" style="width:400px;padding-bottom: 25px;">  @Model.RuleConfirmityStatement </td>
                        </tr>
                    </tbody>
                </table>
            </div>

            <table style="margin-top:15px">
                <tbody>
                    <tr>
                        <td style="width:325px; padding-bottom: 25px; width: 800px;">
                            <div class="col-6 text-start">
                                <h5 class="trn" style="margin-bottom: 5px;">Calibrated By</h5>
