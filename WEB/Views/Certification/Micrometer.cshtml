                                    <img src="../UserSignature/@Model.MicrometerData.ReviewedBySign" style="width: 100px;"/>
                                    <p style="margin: 0px;">@Model.MicrometerData.ReviewedBy</p>
                                    <p style="margin: 0px;">@Model.MicrometerData.ReviewedByDesignation</p>
                                </td>
                            </tr>
                        </tbody>
                    </table>

                </div>
                <div style="border-collapse: collapse; 
                    border-top: solid;
                    border-block-width: 2px; border: 2px solid #000; display: flex;">
                    <div style="padding:10px">
