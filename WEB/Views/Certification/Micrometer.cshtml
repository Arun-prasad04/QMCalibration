                </tr>
            </tbody>
        </table>
        <div>
            <h2 style="text-align: center; text-transform: uppercase;">Certificate of calibration</h2>
        </div>
        <div style="border-collapse: collapse; border:2px solid #000">
            <div>
                <div style="padding: 10px;">
                    <div>
                        <p style="width:325px; font-weight: 900;padding-bottom: 25px; width: 600px;">I. Descriptions of DUC</p>
                    </div>
                    <table>
                        <tbody>
                            <tr>
                                <td style="width:325px; font-weight: 900;padding-bottom: 25px;">Certificate No</td>
                                <td style="width:400px;padding-bottom: 25px;">:  @Model.MicrometerData.CertificateFormat</td>
                                <td style="width:325px; font-weight: 900;padding-bottom: 25px;">Report Issues Date</td>
                                <td style="width:400px;padding-bottom: 25px;">: @DateTime.Now.ToString("dd-MM-yyyy")</td>
                            </tr>
                            @if(Model.IsNABL ?? false)
                            {
                                <tr>
                                    <td style="width:325px; font-weight: 900;padding-bottom: 25px;">ULR No</td>
                                    <td style="width:400px;padding-bottom: 25px;">: @Model.MicrometerData.ULRFormat</td>
                                    <td style="width:325px; font-weight: 900;padding-bottom: 25px;"></td>
                                    <td style="width:400px;padding-bottom: 25px;"></td>
                                </tr>
                            }
                            <tr>
                                <td style="width:325px; font-weight: 900; padding-bottom: 25px;">Date of Calibration</td>
                                <td style="width:400px; padding-bottom: 25px;">: @Model.MicrometerData.CalibrationPerformedDate.ToString("dd-MM-yyyy")</td>
                                <td style="width:325px; font-weight: 900; padding-bottom: 25px;">Next Calibration Due</td>
                                @if(Model.Result == "Rejected")
                                    {
                                        <td style="width:400px; padding-bottom: 25px;">: --</td>
                                    }
                                    else
                                    {
                                <td style="width:400px; padding-bottom: 25px;">: @Model.DueDate.ToString("MMM-yyyy")</td>
                                    }
                            </tr>
                            <tr>
                                <td style="width:325px; font-weight: 900; padding-bottom: 25px;">Name of the Customer</td>
                                <td style="width:400px; padding-bottom: 25px;">: @Model.DepartmentName</td>
                                <td style="width:325px; font-weight: 900;padding-bottom: 25px;"></td>
                                <td style="width:400px;padding-bottom: 25px;"></td>
                            </tr>
                            <tr>
                                <td style="width:325px; font-weight: 900;padding-bottom: 25px;">Department</td>
                                <td style="width:400px;padding-bottom: 25px;">: @Model.DepartmentName</td>
                                <td style="width:325px; font-weight: 900; padding-bottom: 25px;"></td>
                                <td style="width:400px; padding-bottom: 25px;"></td>
                            </tr>
                            <tr>
                                <td style="width:325px; font-weight: 900; padding-bottom: 25px;">Date of Receipt</td>
                                <td style="width:400px; padding-bottom: 25px;">: @Model.DateOfReceipt.ToString("dd-MM-yyyy")</td>
                                <td style="width:325px; font-weight: 900; padding-bottom: 25px;"></td>
                                <td style="width:400px; padding-bottom: 25px;"></td>
                            </tr>
                            <tr>
                                <td style="width:325px; font-weight: 900; padding-bottom: 25px;">Description of Equipment</td>
                                <td style="width:400px; padding-bottom: 25px;">: @Model.InstrumentName</td>
                                <td style="width:325px; font-weight: 900; padding-bottom: 25px;"></td>
                                <td style="width:400px; padding-bottom: 25px;"></td>
                            </tr>
                        </tbody>
                    </table>


