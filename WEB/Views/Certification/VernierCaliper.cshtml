                        <th style="border-collapse: collapse; border:1px solid #000; text-align: center; width:325px;height:25px; padding:5px" rowspan="2" class="text-center">Deviation</th>
						<th style="border-collapse: collapse; border:1px solid #000; text-align: center; width:325px;height:25px; padding:5px" colspan="2" class="text-center">Depth Measurement</th>
                        <th style="border-collapse: collapse; border:1px solid #000; text-align: center; width:325px;height:25px; padding:5px" rowspan="2" class="text-center">Deviation</th>
                    </tr>
					<tr>
                        <th style="border-collapse: collapse; border:1px solid #000; text-align: center; width:325px;height:25px; padding:5px" class="text-center">Nominal</th>
                        <th style="border-collapse: collapse; border:1px solid #000; text-align: center; width:325px;height:25px; padding:5px" class="text-center">Actual</th>
                        <th style="border-collapse: collapse; border:1px solid #000; text-align: center; width:325px;height:25px; padding:5px" class="text-center">Nominal</th>
                        <th style="border-collapse: collapse; border:1px solid #000; text-align: center; width:325px;height:25px; padding:5px" class="text-center">Actual</th>	
                        <th style="border-collapse: collapse; border:1px solid #000; text-align: center; width:325px;height:25px; padding:5px" class="text-center">Nominal</th>
                        <th style="border-collapse: collapse; border:1px solid #000; text-align: center; width:325px;height:25px; padding:5px" class="text-center">Actual</th>
                    </tr>
                    
                    <tr id="firstRow">
                        <td style="border-collapse: collapse; border:1px solid #000; text-align: center; width:325px;height:25px; padding:5px" class="text-center">1</td>
                        <td style="border-collapse: collapse; border:1px solid #000; text-align: center; width:325px;height:25px; padding:5px" class="text-center" id="ExternalNominal1">@Model.VernierData.Measured1_1</td>
                        <td style="border-collapse: collapse; border:1px solid #000; text-align: center; width:325px;height:25px; padding:5px" class="text-center" id="ExternalAvg1">@Model.VernierData.Avg1_1</td>
                        <td style="border-collapse: collapse; border:1px solid #000; text-align: center; width:325px;height:25px; padding:5px" class="text-center" id="ExternalDeviation1"></td>
                        <td style="border-collapse: collapse; border:1px solid #000; text-align: center; width:325px;height:25px; padding:5px" class="text-center" id="InternalNominal1">@Model.VernierData.Measured5_1</td>
                        <td style="border-collapse: collapse; border:1px solid #000; text-align: center; width:325px;height:25px; padding:5px" class="text-center" id="InternalAvg1">@Model.VernierData.Avg5_1</td>
                        <td style="border-collapse: collapse; border:1px solid #000; text-align: center; width:325px;height:25px; padding:5px" class="text-center" id="InternalDeviation1"></td>
                        <td style="border-collapse: collapse; border:1px solid #000; text-align: center; width:325px;height:25px; padding:5px" class="text-center" id="DepthNominal1">@Model.VernierData.Measured3_1</td>
                        <td style="border-collapse: collapse; border:1px solid #000; text-align: center; width:325px;height:25px; padding:5px" class="text-center" id="DepthAvg1">@Model.VernierData.Avg3_1</td>
                        <td style="border-collapse: collapse; border:1px solid #000; text-align: center; width:325px;height:25px; padding:5px" class="text-center" id="DepthDeviation1"></td>
                    </tr>                    
                    @if((Model.VernierData.Measured1_2=="0.0" || Model.VernierData.Measured1_2=="" || Model.VernierData.Measured1_2==null) && (Model.VernierData.Avg1_2=="" || Model.VernierData.Avg1_2==null || Model.VernierData.Avg1_2=="0.0000"))
                    {
                    }
                    else{
					<tr id="secondRow">
                        <td style="border-collapse: collapse; border:1px solid #000; text-align: center; width:325px;height:25px; padding:5px" class="text-center">2</td>
                        <td style="border-collapse: collapse; border:1px solid #000; text-align: center; width:325px;height:25px; padding:5px" class="text-center" id="ExternalNominal2">@Model.VernierData.Measured1_2</td>
                        <td style="border-collapse: collapse; border:1px solid #000; text-align: center; width:325px;height:25px; padding:5px" class="text-center" id="ExternalAvg2">@Model.VernierData.Avg1_2</td>
                        <td style="border-collapse: collapse; border:1px solid #000; text-align: center; width:325px;height:25px; padding:5px" class="text-center" id="ExternalDeviation2"></td>
                        <td style="border-collapse: collapse; border:1px solid #000; text-align: center; width:325px;height:25px; padding:5px" class="text-center" id="InternalNominal2">@Model.VernierData.Measured5_2</td>
                        <td style="border-collapse: collapse; border:1px solid #000; text-align: center; width:325px;height:25px; padding:5px" class="text-center" id="InternalAvg2">@Model.VernierData.Avg5_2</td>
                        <td style="border-collapse: collapse; border:1px solid #000; text-align: center; width:325px;height:25px; padding:5px" class="text-center" id="InternalDeviation2"></td>
                        <td style="border-collapse: collapse; border:1px solid #000; text-align: center; width:325px;height:25px; padding:5px" class="text-center" id="DepthNominal2">@Model.VernierData.Measured3_2</td>
                        <td style="border-collapse: collapse; border:1px solid #000; text-align: center; width:325px;height:25px; padding:5px" class="text-center" id="DepthAvg2">@Model.VernierData.Avg3_2</td>
                        <td style="border-collapse: collapse; border:1px solid #000; text-align: center; width:325px;height:25px; padding:5px" class="text-center" id="DepthDeviation2"></td>
                    </tr>
                    }
