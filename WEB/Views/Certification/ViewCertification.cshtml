<body style="font-family: Calibri;">
    <!-- page1 start-->

    <div style="padding-left: 5%; padding-right: 5%; margin-bottom: 5%;">
        <table style="border-collapse: collapse; border:2px solid #000">
            <tbody>
                <tr>
                    <td style="border-collapse: collapse; border:2px solid #000; text-align: center; width:250px">
                        @*@if (Model.IsNABL ?? false)
                        {
                        <img src="../image/NABL.png" style="padding-top:5px;" width="100px" />
                        }
                        else
                        {
                        <img src="../image/Bharat_Benz.png" style="padding-top:5px;" width="150px" />
                        }*@
                        <img src="../image/Fuso.png" style="padding-top:5px;" width="100px" />
                    </td>
                    <td style="border-collapse: collapse; border:2px solid #000; text-align: center; width:840px">
                        <div>
                            <h5 class="trn" style="margin: 5px;font-size:40px;">Mitsubishi Fuso Truck and Bus Corporation</h5>
                            <p class="trn" style="margin: 5px; font-size: 18px;">Ohkuracho 10, Nakahara Ward, Kawasaki City, Kanagawa Prefecture, Japan, 211-8522</p>
                        </div>
                    </td>
                    <td style="border-collapse: collapse; border:2px solid #000; text-align: center; width:100px">
                        <img style="padding-left:40px;padding-top:5px;" id="imgQRCode" height="150px" width="190px"
                             src="@ViewBag.QRCodeImage" />
                    </td>
                </tr>
            </tbody>
        </table>
        @if (@Model.TypeOfEquipment == "Internal Instrument")
        {
            <div>
                <h2 class="trn" style="text-align: center; text-transform: uppercase;">CERTIFICATE OF CALIBRATION</h2>
        </div>
        }
        @if (@Model.TypeOfEquipment == "External Instrument")
        {
            <div>
                <h2 class="trn" style="text-align: center; text-transform: uppercase;">CERTIFICATE OF VERIFICATION</h2>
        </div>
        }
        <div style="border-collapse: collapse; border:2px solid #000">
            <div>
                <div style="padding: 10px;">
                    @if (@Model.TypeOfEquipment == "Internal Instrument")
                    {
                        <div>
                            <p class="trn" style="width:325px; font-weight: 900;padding-bottom: 25px; width: 600px;">I. Description of device under calibration (DUC)</p>
                        </div>
                    }
                    @if (@Model.TypeOfEquipment == "External Instrument")
                    {
                        <div>
                            <p class="trn" style="width:325px; font-weight: 900;padding-bottom: 25px; width: 600px;">I. Description of device under verification (DUC)</p>
                        </div>
                    }                   
                    <table>
                        <tbody>
                            <tr>
                                <td class="trn" style="width:325px; font-weight: 900;padding-bottom: 25px;">Certificate No.</td>
                                <td style="width:400px;padding-bottom: 25px;">: @Model.TempObservationData.CertificateNumber</td>
                                <td class="trn" style="width:340px; font-weight: 900;padding-bottom: 25px;">Report issue date</td>
                                <td style="width:400px;padding-bottom: 25px;">: @Model.CalibrationCloseDate.ToString("dd-MM-yyyy")</td>
                            </tr>
                            <tr>
                                <td class="trn" style="width:325px; font-weight: 900;padding-bottom: 25px;">Date of Calibration</td>
                                <td style="width:400px;padding-bottom: 25px;">: @Model.TempObservationData.CalibrationReviewedDate.ToString("dd-MM-yyyy")</td>
                                <td class="trn" style="width:340px; font-weight: 900;padding-bottom: 25px;">Next Calibrtaion due</td>
                                <td style="width:400px;padding-bottom: 25px;">: @Model.DueDate.ToString("dd-MM-yyyy")</td>
                            </tr>
                            <tr>
                                <td class="trn" style="width:325px; font-weight: 900;padding-bottom: 25px;">Name of Dept.</td>
                                <td style="width:400px;padding-bottom: 25px;">: @Model.DepartmentName.</td>
                                <td class="trn" style="width:340px; font-weight: 900;padding-bottom: 25px;">Date of receipt</td>
                                <td style="width:400px;padding-bottom: 25px;">: @Model.CalibDate.ToString("dd-MM-yyyy")</td>
                            </tr>
                            <tr>
                                <td class="trn" style="width:325px; font-weight: 900;padding-bottom: 25px;">Sub section code</td>
                                <td style="width:400px;padding-bottom: 25px;">: @Model.SubSecCode</td>
                                <td class="trn" style="width:340px; font-weight: 900;padding-bottom: 25px;">Description of equipment</td>
                                <td style="width:400px;padding-bottom: 25px;">: @Model.InstrumentName</td>
                            </tr>
                            <tr>
                                <td class="trn" style="width:325px; font-weight: 900;padding-bottom: 25px;">Identification No.</td>
                                <td style="width:400px;padding-bottom: 25px;">: @Model.IdNo</td>
                                 @if (@Model.TypeOfEquipment == "Internal Instrument")
                                {
                                    <td class="trn" style="width:340px; font-weight: 900;padding-bottom: 25px;">Condition of DUC</td>
                                }
                                @if (@Model.TypeOfEquipment == "External Instrument")
                                {
                                    <td class="trn" style="width:340px; font-weight: 900;padding-bottom: 25px;">Condition of DUV</td>
                                }
                                <td style="width:400px;padding-bottom: 25px;">: @Model.TempObservationData.InstrumentCondition</td>
                            </tr>

                        </tbody>
                    </table>


                    <div style="padding-top: 10px;">
                        <table style="border-collapse: collapse; border:1px solid #000">
                            <tr>
                                <th class="trn" style="border-collapse: collapse; border:1px solid #000; text-align: center; width:825px; padding: 5px;">Range</th>

                                <th class="trn" style="border-collapse: collapse; border:1px solid #000; text-align: center; width:825px; padding: 5px;">Least Count</th>

                                <th class="trn" style="border-collapse: collapse; border:1px solid #000; text-align: center; width:825px; padding: 5px;">Serial No</th>

                                <th class="trn" style="border-collapse: collapse; border:1px solid #000; text-align: center; width:825px; padding: 5px;">Make</th>

                            </tr>
                            <tr>
                                <td style="border-collapse: collapse; border:1px solid #000; text-align: center; width:825px">
                                    <p>@Model.Range</p>
                                </td>
                                <td style="border-collapse: collapse; border:1px solid #000; text-align: center; width:825px">
                                    <p>@Model.LC</p>
                                </td>
                                <td style="border-collapse: collapse; border:1px solid #000; text-align: center; width:825px">
                                    <p>@Model.SlNo</p>
                                </td>
                                <td style="border-collapse: collapse; border:1px solid #000; text-align: center; width:825px">
                                    <p>@Model.Make</p>
                                </td>
                            </tr>
                        </table>

                        <table style="margin-top:15px">
                            <tbody>
                                <tr>
                                    <td class="trn" style="width:325px; font-weight: 900;padding-bottom: 25px;">Calibration procedure/Reference standard</td>
                                    <td style="width:400px;padding-bottom: 25px;">: @Model.StandardReffered</td>
                                    @if(@Model.TypeOfEquipment == "Internal Instrument")
                                    {
                                    <td class="trn" style="width:325px; font-weight: 900;padding-bottom: 25px;">Environmental condition</td>
                                    <td style="width:400px;padding-bottom: 25px;">: 22±2°C/50±10% RH</td>
                                    }
                                    @if (@Model.TypeOfEquipment == "External Instrument")
                                    {
                                        <td class="trn" style="width:325px; font-weight: 900;padding-bottom: 25px;">External</td>
                                        <td style="width:400px;padding-bottom: 25px;"></td>
                                    }

                                </tr>
                            </tbody>
                        </table>
                    </div>
                    
                        <div>
                            <p class="trn" style="width:325px; font-weight: 900; padding-bottom:25px; width:600px;">II. Master & Equipment used</p>
                        </div>
                        <div style="margin-top: 10px;">
                            <table style="border-collapse: collapse; border:1px solid #000">
                                <tr>
                                    <th class="trn" style="border-collapse: collapse; border:1px solid #000; text-align: center; width:325px">SI.</th>


                                    <th class="trn" style="border-collapse: collapse; border:1px solid #000; text-align: center; width:325px">Master Equipment</th>

                                    <th class="trn" style="border-collapse: collapse; border:1px solid #000; text-align: center; width:325px">Id No</th>

                                    <th class="trn" style="border-collapse: collapse; border:1px solid #000; text-align: center; width:325px">Make</th>

                                    <th class="trn" style="border-collapse: collapse; border:1px solid #000; text-align: center; width:325px">Traceability</th>

                                    <th class="trn" style="border-collapse: collapse; border:1px solid #000; text-align: center; width:325px">Certificate No</th>

                                    <th class="trn" style="border-collapse: collapse; border:1px solid #000; text-align: center; width:325px">Validity</th>

                                </tr>
                                @{
                            @if (@Model.TypeOfEquipment == "Internal Instrument")
                            {
                                
                                    int i = 1;
                                    @foreach (var item in @Model.MasterEqiupmentList)
                                    {
                                        <tr>
                                            <td style="border-collapse: collapse; border:1px solid #000; text-align: center; width:325px">@i</td>
                                            <td style="border-collapse: collapse; border:1px solid #000; text-align: center; width:325px">@item.EquipName</td>
                                            <td style="border-collapse: collapse; border:1px solid #000; text-align: center; width:325px">@item.LabId</td>
                                            <td style="border-collapse: collapse; border:1px solid #000; text-align: center; width:325px">@item.Make</td>
                                            <td style="border-collapse: collapse; border:1px solid #000; text-align: center; width:325px">@item.Traceability</td>
                                            <td style="border-collapse: collapse; border:1px solid #000; text-align: center; width:325px">@item.CertNo</td>
                                            <td style="border-collapse: collapse; border:1px solid #000; text-align: center; width:325px">@item.DueDate.ToString("dd-MM-yyyy")</td>
                                        </tr>
                                        i++;
                                    }
                                
                            }
                            @if (@Model.TypeOfEquipment == "External Instrument")
                            {
                                 <td style="border-collapse: collapse; border:1px solid #000; text-align: center; width:325px">1</td>
                                    <td colspan="6" style="border-collapse: collapse; border:1px solid #000; text-align: center; width:325px;">Refer external calibartion report</td>
                            }
                            }
                        </table>

                        </div>
                    
                    <div>
                        <table style="margin-top:15px">
                            <tbody>
                                <tr>
                                    <td class="trn" style="width:325px; font-weight: 900;padding-bottom: 25px;">III. Calibration Results</td>
                                    <td style="width:400px;padding-bottom: 25px;"></td>

                                    <td class="trn" style="width:325px; font-weight: 900;padding-bottom: 25px;">All values are in</td>
                                    <td style="width:400px;padding-bottom: 25px;">: @Model.TempObservationData.Allvalues</td>
                                </tr>
                            </tbody>
                        </table>
                    </div>

                    @*<div>
                    <table style="border-collapse: collapse; border:1px solid #000">
                    </table>
                    </div>*@

                    @if(@Model.TypeOfEquipment == "Internal Instrument")
                    {
                        <div id="dvTable" class="col-md-8 content"></div>
                    }
                    

                    @if (@Model.TypeOfEquipment == "External Instrument")
                    {
                        <div id="dvExtTable" class="content" style="align-content:center">
                            <table style="border: 1px solid; width: 100%; border-collapse: collapse;">
                                <tr>
                                    <td class="trn" style="border: 1px solid; text-align:center">External report result status</td>
                                    <td class="trn" style="border: 1px solid; text-align:center">1</td>
                                    <td class="trn" style="border: 1px solid; text-align:center"><input type="radio" checked="checked" id="checkOk" name="radio"> OK</td>
                                    <td class="trn" style="border: 1px solid; text-align:center"><input type="radio" checked="checked" id="checkNotOk" name="radio"> Not OK</td>
                                </tr>
                            </table>
                        </div>
                    }
                    @if (@Model.TypeOfEquipment == "External Instrument")
                    {
                        <p class="trn" style="margin-top:15px">Refer the external calibartion report for more details</p>
                    }
                    <table style="margin-top:15px">
                        <tbody>
                            <tr>
                                <td class="trn" style="width:325px; font-weight: 900;padding-bottom: 25px;">IV. Result Of Calibration :</td>

                                <td class="trn" style="width:400px;padding-bottom: 25px;">
                                    <input class="form-control"
                                           placeholder="Result Of Calibration" id="CalibrationResult" name="CalibrationResult"
                                           value="@Model.TempObservationData.CalibrationResult"> @*Model.TempObservationData.CalibrationResult*@
                                </td>
                                <td style="width:325px; font-weight: 900;padding-bottom: 25px;"></td>
                                <td style="width:400px;padding-bottom: 25px;"></td>
                            </tr>
                            <tr>
                                <td class="trn" style="width:325px; font-weight: 900;padding-bottom: 25px;">V. Remarks :</td>
                                <td class="trn" style="width:400px;padding-bottom: 25px;">
                                    <input class="form-control"
                                           placeholder="Remarks" id="Remarks" name="Remarks"
                                           value="@Model.TempObservationData.Remarks"> @*Model.TempObservationData.Remarks*@
                                </td>
                                <td style="width:325px; font-weight: 900;padding-bottom: 25px;"></td>
                                <td style="width:400px;padding-bottom: 25px;"></td>
                            </tr>
                        </tbody>
                    </table>

                    @if(@Model.TypeOfEquipment == "Internal Instrument")
                    {
                        <p class="trn">Note: Statement of conformity is given based on decision rule agreed by requestor</p>
                    }
                    @if(@Model.TypeOfEquipment == "External Instrument")
                    {
                        <p class="trn">Note: Statement of conformity is given based on decision rule agreed by requestor and external certificate</p>
                    }


                    <table style="margin-top:15px">
                        <tbody>
                            <tr>
                                <td style="width:325px; padding-bottom: 25px; width: 800px;">
                                    <div class="col-6 text-start">
                                        <h5 class="trn" style="margin-bottom: 5px;">Calibrated By</h5>
                                        <img src="../wwwroot/UserSignature/Guru sign.jpg" />
                                        <p style="margin: 0px;">@Model.TempObservationData.CalibrationPerformedBy</p>
                                        <p style="margin: 0px;">@Model.TempObservationData.PerformedByDesignation</p>
                                    </div>
                                </td>
                                <td style="width:400px;padding-bottom: 25px; width: 800px; text-align: right;">
                                    <h5 class="trn" style="margin-bottom: 5px;">Authorized Signatory</h5>
                                    <img src="../wwwroot/UserSignature/Gurushev.jpg" />
                                    <p style="margin: 0px;">@Model.TempObservationData.ReviewedBy</p>
                                    <p style="margin: 0px;">@Model.TempObservationData.ReviewedByDesignation</p>
                                </td>
                            </tr>
                        </tbody>
                    </table>

                </div>
                @if(@Model.TypeOfEquipment == "Internal Instrument")
                    {
                <div style="border-collapse: collapse;border-top: solid;border-block-width: 2px; border: 2px solid #000; display: flex;">
                    <div style="padding:10px">
                        <p class="trn" style="margin: 0px; font-size: 12px;">1. The calibration results are valid at the time of and under the stated condition of measurement.</p>
                        <p class="trn" style="margin: 0px; font-size: 12px;">2. The report should not be reproduced either in full or in part without permission in writing.</p>
                        <p class="trn" style="margin: 0px; font-size: 12px;">3. Next calibration due date given in this certificate as per requestor irrespective of the status or condition of instrument.</p>
                    </div>
                </div>
                }
                 @if(@Model.TypeOfEquipment == "External Instrument")
                {
                    <div style="border-collapse: collapse;border-top: solid;border-block-width: 2px; border: 2px solid #000; display: flex;">
                        <div style="padding:10px">
                            <p class="trn" style="margin: 0px; font-size: 12px;">1. The calibration results based on external certificate result</p>
                            <p class="trn" style="margin: 0px; font-size: 12px;">2. Next calibration due date given in this certificate as per requestor irrespective of the status or condition of instrument.</p>                           
                        </div>
                    </div>
                }
            </div>
        </div>
        @*<table style="margin-top:15px">
            <tbody>
                <tr style="font-size: xx-small;">
                    <td style="width:325px; padding-bottom: 25px; width: 800px;">
                        <div>
                            <h4 class="trn" style="font-size: small;">Format No : 365.QM.D.16, Rev No : 02</h4>
                        </div>
                    </td>
                    <td style="width:400px;padding-bottom: 25px; width: 800px; text-align: right;">
                        <h4 class="trn" style="font-size: small;">Page 1 of 2</h4>
                    </td>
                </tr>
            </tbody>
        </table>*@
    </div>

    @*<input type="hidden" id="ReviewStatus" name="ReviewStatus" value="@Model.ReviewStatus">*@
    <input type="hidden" id="InstrumentId" name="InstrumentId" value="@Model.Id">
    <input type="hidden" id="RequestId" name="RequestId" value="@Model.RequestId">
    <input type="hidden" id="ExternalObsStatus" name="ExternalObsStatus" value="@Model.TempObservationData.ExternalObsStatus">
    @*<input type="hidden" id="TemplateObservationId" name="TemplateObservationId" value="@Model.TemplateObservationId">*@

    <!-- page1 end-->

    <div>
        <input type="hidden" id="requestId" value="@ViewBag.RequestId">
        <input type="hidden" id="instrumentId" value="@ViewBag.InstrumentId">
        <input type="hidden" id="isDownloadPdf" value="@ViewBag.IsDownloadPdf">
        <input type="hidden" name="FileName" id="FileName" value="@ViewBag.FileName" />
        @if (@Model.isExportCertificate)
        {
            @if (@HttpContextAccessor.HttpContext.Session.GetString("UserRoleId") == "2")
            {
                <input type="button" value="ReGenerate" onclick="SaveCertificateTemp(language)" class="btn btn-primary trn">
            }
            <input type="button" class="btn btn-primary trn" id="btnExport" name="btnExport" value="Export" onclick="downloadPdfFile()" />
            <a class="btn btn-primary trn" href='@Url.Action("QRCodePrint","Certification", new {requestId=@ViewBag.RequestId, instrumentId=@ViewBag.InstrumentId})'>PrintQR Code</a>
        }
        else
        {
            @if (@HttpContextAccessor.HttpContext.Session.GetString("UserRoleId") == "2")
            {
                <input type="button" value="Save" onclick="SaveCertificateTemp(language)" class="btn btn-primary">
            }
        }
        <a class="btn btn-primary trn" href='@Url.Action("Request","Tracker",new {reqType=4})'>Cancel</a>
    </div>
    </div>
</body>


