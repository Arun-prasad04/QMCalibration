@using Microsoft.AspNetCore.Http
@inject Microsoft.AspNetCore.Http.IHttpContextAccessor HttpContextAccessor
<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>

<body style="font-family: Calibri;">
    <!-- page1 start-->
    <div style="padding-left: 5%; padding-right: 5%; margin-bottom: 5%;">
        <table style="border-collapse: collapse; border:2px solid #000">
            <tbody>
                <tr>
                    <td style="border-collapse: collapse; border:2px solid #000; text-align: center; width:250px">
                    @if(Model.IsNABL ?? false)
                    {
                        <img src="../image/NABL.png" style="padding-top:5px;"  width="100px" />
                    }
                    else
                    {
                        <img src="../image/Bharat_Benz.png" style="padding-top:5px;" width="150px" />
                    }
                        
                    </td>
                    <td style="border-collapse: collapse; border:2px solid #000; text-align: center; width:840px">
                        <div>
                            <h5 style="margin: 5px;" class="trn">Daimler India Commercial Vehicles Pvt. Ltd -Central Lab</h5>
                            <p style="margin: 5px; font-size: 12px;" class="trn">SIPCOT Insdustrial Estate, Oragadam Village,Singaperumal Koil Road,</p>
                            <p style="margin: 5px; font-size: 12px;" class="trn">Mathur(PO), Sriperumbadur Taluk, Kanchipuram -602105, Tamil Nadu, India.</p>
                            <p style="margin: 5px; font-size: 12px;" class="trn">Phone: +91 44 4969 4456,</p>
                            <p style="margin: 5px; font-size: 12px;" class="trn">Website: www.daimler-indiacv.com / .bharatbenz.com</p>
                        </div>
                    </td>
                    <td style="border-collapse: collapse; border:2px solid #000; text-align: center; width:100px">
                        <img style="padding-left:40px;padding-top:5px;" id="imgQRCode" height="150px" width="190px" src="@ViewBag.QRCodeImage" />
                    </td>
                </tr>
            </tbody>
        </table>
        <div>
            <h2 style="text-align: center; text-transform: uppercase;" class="trn">Certificate of calibration</h2>
        </div>
        <div style="border-collapse: collapse; border:2px solid #000">
            <div>
                <div style="padding: 10px;">
                    <div>
                        <p style="width:325px; font-weight: 900;padding-bottom: 25px; width: 600px;" class="trn">I. Descriptions of DUC</p>
                    </div>
                    <table>
                        <tbody>
                            <tr>
                                <td style="width:325px; font-weight: 900;padding-bottom: 25px;" class="trn">Certificate No</td>
                                <td style="width:400px;padding-bottom: 25px;">: @Model.TorqueData.CertificateFormat</td>
                                <td style="width:325px; font-weight: 900;padding-bottom: 25px;" class="trn">Report Issues Date</td>
                                <td style="width:400px;padding-bottom: 25px;">: @DateTime.Now.ToString("dd-MM-yyyy")</td>
                            </tr>
                            @if(Model.IsNABL ?? false)
                            {
                                <tr>
                                    <td style="width:325px; font-weight: 900;padding-bottom: 25px;" class="trn">ULR No</td>
                                    <td style="width:400px;padding-bottom: 25px;">: @Model.TorqueData.ULRFormat</td>
                                    <td style="width:325px; font-weight: 900;padding-bottom: 25px;"></td>
                                    <td style="width:400px;padding-bottom: 25px;"></td>
                                </tr>
                            }
                            <tr>
                                <td style="width:325px; font-weight: 900; padding-bottom: 25px;" class="trn">Date of Calibration</td>
                                <td style="width:400px; padding-bottom: 25px;">: @Model.TorqueData.CalibrationPerformedDate.ToString("dd-MM-yyyy")</td>
                                <td style="width:325px; font-weight: 900; padding-bottom: 25px;" class="trn">Next Calibration Due</td>
                                @if(Model.Result == "Rejected")
                                {
                                    <td style="width:400px; padding-bottom: 25px;">: --</td>
                                }
                                else
                                {
                                    <td style="width:400px; padding-bottom: 25px;">: @Model.DueDate.ToString("MMM-yyyy")</td>
                                }
                            </tr>
                            <tr>
                                <td style="width:325px; font-weight: 900; padding-bottom: 25px;" class="trn">Name of the Customer</td>
                                <td style="width:400px; padding-bottom: 25px;">: @Model.DepartmentName</td>
                                <td style="width:325px; font-weight: 900;padding-bottom: 25px;"></td>
                                <td style="width:400px;padding-bottom: 25px;"></td>
                            </tr>
                            <tr>
                                <td style="width:325px; font-weight: 900;padding-bottom: 25px;" class="trn">Department</td>
                                <td style="width:400px;padding-bottom: 25px;">: @Model.DepartmentName</td>
                                <td style="width:325px; font-weight: 900; padding-bottom: 25px;"></td>
                                <td style="width:400px; padding-bottom: 25px;"></td>
                            </tr>
                            <tr>
                                <td style="width:325px; font-weight: 900; padding-bottom: 25px;" class="trn">Date of Receipt</td>
                                <td style="width:400px; padding-bottom: 25px;">: @Model.DateOfReceipt.ToString("dd-MM-yyyy")</td>
                                <td style="width:325px; font-weight: 900; padding-bottom: 25px;"></td>
                                <td style="width:400px; padding-bottom: 25px;"></td>
                            </tr>
                            <tr>
                                <td style="width:325px; font-weight: 900; padding-bottom: 25px;" class="trn">Description of Equipment</td>
                                <td style="width:400px; padding-bottom: 25px;">: @Model.InstrumentName</td>
                                <td style="width:325px; font-weight: 900; padding-bottom: 25px;"></td>
                                <td style="width:400px; padding-bottom: 25px;"></td>
                            </tr>
                        </tbody>
                    </table>


                    <div style="padding-top: 10px;">
                        <table style="border-collapse: collapse; border:1px solid #000">
                            <tr>
                                <th style="border-collapse: collapse; border:1px solid #000; text-align: center; width:825px; padding: 5px;" class="trn">Range</th>
                                <th style="border-collapse: collapse; border:1px solid #000; text-align: center; width:825px; padding: 5px;" class="trn">Least Count</th>
                                <th style="border-collapse: collapse; border:1px solid #000; text-align: center; width:825px; padding: 5px;" class="trn">Serial No</th>
                                <th style="border-collapse: collapse; border:1px solid #000; text-align: center; width:825px; padding: 5px;" class="trn">Make</th>
                            </tr>
                            <tr>
                                <td style="border-collapse: collapse; border:1px solid #000; text-align: center; width:825px">
                                    <p>@Model.Range</p>
                                </td>
                                <td style="border-collapse: collapse; border:1px solid #000; text-align: center; width:825px">
                                    <p>@Model.LC</p>
                                </td>
                                <td style="border-collapse: collapse; border:1px solid #000; text-align: center; width:825px">
                                    <p>@Model.SlNo</p>
                                </td>
                                <td style="border-collapse: collapse; border:1px solid #000; text-align: center; width:825px">
                                    <p>@Model.Make</p>
                                </td>
                            </tr>
                        </table>

                        <table style="margin-top:15px">
                            <tbody>
                                <tr>
                                    <td style="width:325px; font-weight: 900;padding-bottom: 25px;" class="trn">Identification Number</td>
                                    <td style="width:400px;padding-bottom: 25px;">: @Model.IdNo</td>
                                    <td style="width:325px; font-weight: 900;padding-bottom: 25px;"></td>
                                    <td style="width:400px;padding-bottom: 25px;"></td>
                                </tr>
                                <tr>
                                    <td style="width:325px; font-weight: 900;padding-bottom: 25px;" class="trn">Condition of DUC</td>
                                    <td style="width:400px;padding-bottom: 25px;">: @Model.TorqueData.ConditionOfTW</td>
                                    <td style="width:325px; font-weight: 900;padding-bottom: 25px;"></td>
                                    <td style="width:400px;padding-bottom: 25px;"></td>
                                </tr>
                                <tr>
                                    <td style="width:325px; font-weight: 900; padding-bottom: 25px;" class="trn">Calibration Procedure / <br/> Reference Standard :</td>
                                    <td style="width:400px; padding-bottom: 25px;">: @Model.StandardReffered</td>
                                    <td style="width:325px; font-weight: 900; padding-bottom: 25px;"></td>
                                    <td style="width:400px; padding-bottom: 25px;"></td>
                                </tr>
                            </tbody>
                        </table>
                    </div>

                    <div>
                        <p style="width:325px; font-weight: 900;padding-bottom: 25px; width: 600px;" class="trn">II. Master & Equipment Used For Calibration & Traceability</p>
                    </div>
                    <div style="margin-top: 10px;">
                        <table style="border-collapse: collapse; border:1px solid #000">
                            <tr>
                                <th style="border-collapse: collapse; border:1px solid #000; text-align: center; width:325px" class="trn">SI.</th>
                                <th style="border-collapse: collapse; border:1px solid #000; text-align: center; width:325px" class="trn">Master Equipment</th>
                                <th style="border-collapse: collapse; border:1px solid #000; text-align: center; width:325px" class="trn">Id No</th>
                                <th style="border-collapse: collapse; border:1px solid #000; text-align: center; width:325px" class="trn">Make </th>
                                <th style="border-collapse: collapse; border:1px solid #000; text-align: center; width:325px" class="trn">Traceability </th>
                                <th style="border-collapse: collapse; border:1px solid #000; text-align: center; width:325px" class="trn">Certificate No </th>
                                <th style="border-collapse: collapse; border:1px solid #000; text-align: center; width:325px" class="trn">Validity </th>
                            </tr>
                            @{ int i=1;
                            @foreach (var item in @Model.MasterEqiupmentList)
                            {
                            <tr>
                                <td style="border-collapse: collapse; border:1px solid #000; text-align: center; width:325px">@i</td>
                                <td style="border-collapse: collapse; border:1px solid #000; text-align: center; width:325px">@item.EquipName</td>
                                <td style="border-collapse: collapse; border:1px solid #000; text-align: center; width:325px">@item.LabId</td>
                                <td style="border-collapse: collapse; border:1px solid #000; text-align: center; width:325px">@item.Make</td>
                                <td style="border-collapse: collapse; border:1px solid #000; text-align: center; width:325px">@item.Traceability</td>
                                <td style="border-collapse: collapse; border:1px solid #000; text-align: center; width:325px">@item.CertNo</td>
                                <td style="border-collapse: collapse; border:1px solid #000; text-align: center; width:325px">@item.DueDate.ToString("dd-MM-yyyy")</td>
                            </tr>
                            i++;
                            }
                           }
                        </table>
                        <p class="fw-normal fontsize14 trn">The master Equipment used for calibration are calibrated and traceable to National / International Standard</p>
                    </div>

                    <table style="margin-top:15px">
                        <tbody>
                            <tr>
                                <td style="width:325px; font-weight: 900;padding-bottom: 25px;" class="trn">Environmental Condition:</td>
                                <td style="width:400px;padding-bottom: 10px;"><p>20 &plusmn; 1.0<sup>o</sup>C / 50 &plusmn; 10% RH</p></td>
                                <td style="width:325px; font-weight: 900;padding-bottom: 25px;"></td>
                                <td style="width:400px;padding-bottom: 25px;"></td>
                            </tr>
                            <tr>
                                <td style="width:325px; font-weight: 900;padding-bottom: 25px;" class="trn">Uncertainty Of Measurement</td>
                                <td style="width:400px;padding-bottom: 25px;"><input class="form-control" placeholder="Uncertainty Of Measurement" id="Uncertainity" name="Uncertainity" value="@Model.TorqueData.Uncertainity"></td>
                                <td style="width:325px; font-weight: 900;padding-bottom: 25px;"></td>
                                <td style="width:400px;padding-bottom: 25px;"></td>
                            </tr>
                        </tbody>
                    </table>

                    <p class="trn">The stated Expanded uncertainty is estimatd at a confidence level approximately 95% with a coverage factor k = 2</p>

                    <table style="margin-top:15px">
                        <tbody>
                            <tr>
                                <td style="width:325px; padding-bottom: 25px; width: 800px;">
                                    <div class="col-6 text-start">
                                        <h5 style="margin-bottom: 5px;" class="trn">Calibrated By</h5>
                                        <img src="../UserSignature/@Model.TorqueData.PerformedBySign" style="width: 100px;"/>
                                        <p style="margin-left:20px;">@Model.TorqueData.CalibrationPerformedBy</p>
                                        <p style="margin-left:20px;">@Model.TorqueData.PerformedByDesignation</p>
                                    </div>
                                </td>
                                <td style="width:400px;padding-bottom: 25px; width: 800px; text-align: right;">
                                    <h5 style="margin-bottom: 5px;" class="trn">Authorized Signatory</h5>
                                    <img src="../UserSignature/@Model.TorqueData.ReviewedBySign" style="width: 100px;"/>
                                    <p style="margin: 0px;">@Model.TorqueData.ReviewedBy</p>
                                    <p style="margin: 0px;">@Model.TorqueData.ReviewedByDesignation</p>
                                </td>
                            </tr>
                        </tbody>
                    </table>

                </div>
                <div style="border-collapse: collapse; 
                    border-top: solid;
                    border-block-width: 2px; border: 2px solid #000; display: flex;">
                    <div style="padding:10px">
                        <p style="margin: 0px; font-size: 12px;" class="trn">1. The Calibration results are valid at the time of & under the stated conditions of measurement.</p>
                        <p style="margin: 0px; font-size: 12px;" class="trn">2. The report shoult not be reproduced either in full or in part without out permission in writing.</p>
                        <p style="margin: 0px; font-size: 12px;" class="trn">3. Next calibration due date given in this cetificate as per customer request irrespective of the status or condition of the IMTE.</p>
                    </div>
                    <div><img style="padding:10px;" src="../image/footer1.png" alt="" height="60px" width="250px"></div>
                </div>
            </div>
        </div>
        <table style="margin-top:15px">
                <tbody>
                    <tr style="font-size: xx-small;">
                        <td style="width:325px; padding-bottom: 25px; width: 800px;">
                            <div>
                            <h4 style="font-size: small;"><span class="trn">Format No :</span> 365.QM.D.16, Rev No : 02</h4>
                            </div>
                        </td>
                        <td style="width:400px;padding-bottom: 25px; width: 800px; text-align: right;">
                        <h4 style="font-size: small;" class="trn">Page 1 of 2</h4>
                        </td>
                    </tr>
                </tbody>
            </table>
    </div>
    <!-- page1 end-->
    <!-- page2 start-->
    <div style="padding-left: 5%; padding-right: 5%; margin-bottom: 5%;">
        <table style="border-collapse: collapse; border:2px solid #000">
            <tbody>
                <tr>
                    <td style="border-collapse: collapse; border:2px solid #000; text-align: center; width:250px">
                       @if(Model.IsNABL ?? false)
						{
							<img src="../image/NABL.png" style="padding-top:5px;"  width="100px" />
						}
						else
						{
							<img src="../image/Bharat_Benz.png" style="padding-top:5px;"  width="150px" />
						}
                    </td>
                    <td style="border-collapse: collapse; border:2px solid #000; text-align: center; width:840px">
                        <div>
                            <h5 style="margin: 5px;" class="trn">Daimler India Commercial Vehicles Pvt. Ltd -Central Lab</h5>                           
                            <p style="margin: 5px; font-size: 12px;" class="trn">SIPCOT Insdustrial Estate, Oragadam Village,Singaperumal Koil Road,</p>
                            <p style="margin: 5px; font-size: 12px;" class="trn">Mathur(PO), Sriperumbadur Taluk, Kanchipuram -602105, Tamil Nadu, India.</p>
                            <p style="margin: 5px; font-size: 12px;" class="trn">Phone: +91 44 4969 4456,</p>
                            <p style="margin: 5px; font-size: 12px;" class="trn">Website: www.daimler-indiacv.com / .bharatbenz.com</p>
                        </div>
                    </td>
                    <td style="border-collapse: collapse; border:2px solid #000; text-align: center; width:100px">
                        <img style="padding-left:40px;padding-top:5px;" id="imgQRCode" height="150px" width="190px" src="@ViewBag.QRCodeImage" />
                    </td>
                </tr>
            </tbody>
        </table>
        <div style="border: 2px solid #000; padding: 10px;">
            <table style="margin-top:30px">
                <tbody>
                    <tr>
                        <td style="width:325px; font-weight: 900;padding-bottom: 25px; width: 600px;" class="trn">III. Mechanical Calibration Results</td>
                        <td style="width:400px;padding-bottom: 25px; width: 600px; text-align: center;"><span class="trn">Certificate No</span> @Model.TorqueData.CertificateFormat</td>
                        <td style="width:325px; font-weight: 900;padding-bottom: 25px; width: 600px; text-align: center;"><span class="trn">Report Issue Date :</span> @DateTime.Now.ToString("dd-MM-yyyy")</td>
                    </tr>
                </tbody>
            </table>

            <div class="p-3">
                @* <p style="text-align: center;">All Values are in Microns</p> *@
                @if(@Model.ObservationType==83){
                <table style="border-collapse: collapse; border:1px solid #000">
                   <tr>
                        <th style="border-collapse: collapse; border:1px solid #000; text-align: center; width:100%;height:25px; padding:5px" colspan="10" class="text-center trn">SPECS</th>
                    </tr>
					<tr>
                        <td style="border-collapse: collapse; border:1px solid #000; text-align: center; width:100%;height:25px; padding:5px" colspan="10" class="text-center"><span class="trn">All Values are in</span> @Model.TorqueData.Allvalues</td>
                    </tr>
					<tr>
                        <td style="border-collapse: collapse; border:1px solid #000; text-align: center; width:325px;height:25px; padding:5px" class="text-center trn" colspan="2">Min Range</td>
                        <td style="border-collapse: collapse; border:1px solid #000; text-align: center; width:325px;height:25px; padding:5px" class="text-center trn" colspan="2">Max Range</td>
                        <td style="border-collapse: collapse; border:1px solid #000; text-align: center; width:325px;height:25px; padding:5px" class="text-center" colspan="2">I</td>
                        <td style="border-collapse: collapse; border:1px solid #000; text-align: center; width:325px;height:25px; padding:5px" class="text-center" colspan="2">II</td>
						<td style="border-collapse: collapse; border:1px solid #000; text-align: center; width:325px;height:25px; padding:5px" class="text-center" colspan="2">III</td>
                    </tr>
					<tr>
                        <td style="border-collapse: collapse; border:1px solid #000; text-align: center; width:325px;height:25px; padding:5px" class="text-center" colspan="2">0</td>
                        <td style="border-collapse: collapse; border:1px solid #000; text-align: center; width:325px;height:25px; padding:5px" class="text-center" colspan="2">@Model.Range</td>
                        <td style="border-collapse: collapse; border:1px solid #000; text-align: center; width:325px;height:25px; padding:5px" class="text-center" colspan="2">20%</td>
                        <td style="border-collapse: collapse; border:1px solid #000; text-align: center; width:325px;height:25px; padding:5px" class="text-center" colspan="2">60%</td>
						<td style="border-collapse: collapse; border:1px solid #000; text-align: center; width:325px;height:25px; padding:5px" class="text-center" colspan="2">100%</td>
                    </tr>
					<tr>
                        <td style="border-collapse: collapse; border:1px solid #000; text-align: center; width:325px;height:25px; padding:5px" class="text-center trn" colspan="2">Total Range</td>
                        <td style="border-collapse: collapse; border:1px solid #000; text-align: center; width:325px;height:25px; padding:5px" class="text-center" colspan="2">@Model.Range</td>
                        <td style="border-collapse: collapse; border:1px solid #000; text-align: center; width:325px;height:25px; padding:5px" class="text-center" colspan="2">@Model.TorqueData.Nominal20</td>
                        <td style="border-collapse: collapse; border:1px solid #000; text-align: center; width:325px;height:25px; padding:5px" class="text-center" colspan="2">@Model.TorqueData.Nominal60</td>
						<td style="border-collapse: collapse; border:1px solid #000; text-align: center; width:325px;height:25px; padding:5px" class="text-center" colspan="2">@Model.TorqueData.Nominal100</td>
                    </tr>
					<tr>
                            <td style="border-collapse: collapse; border:1px solid #000; text-align: center; width:325px;height:25px; padding:5px" class="text-center trn" colspan="4">Acceptable Band</td>
                        <td style="border-collapse: collapse; border:1px solid #000; text-align: center; width:325px;height:25px; padding:5px" class="text-center" colspan="2">@Model.TorqueData.Spec20</td>
                        <td style="border-collapse: collapse; border:1px solid #000; text-align: center; width:325px;height:25px; padding:5px" class="text-center" colspan="2">@Model.TorqueData.Spec60</td>
                        <td style="border-collapse: collapse; border:1px solid #000; text-align: center; width:325px;height:25px; padding:5px" class="text-center" colspan="2">@Model.TorqueData.Spec100</td>
                    </tr>
                </table>
				<table style="margin-top:15px">
                <tbody>
                    <tr>
                        <td style="width:325px; padding-bottom: 25px; width: 800px;">
                        </td>
                    </tr>
                </tbody>
            </table>
                <table style="border-collapse: collapse; border:1px solid #000">
					<tr>
                        <td style="border-collapse: collapse; border:1px solid #000; text-align: center; width:325px;height:25px; padding:5px" class="text-center trn" colspan="2">S.No</td>
                        <td style="border-collapse: collapse; border:1px solid #000; text-align: center; width:325px;height:25px; padding:5px" class="text-center trn" colspan="2">Observed value I</td>
                        <td style="border-collapse: collapse; border:1px solid #000; text-align: center; width:325px;height:25px; padding:5px" class="text-center trn" colspan="2">Observed value II</td>
                        <td style="border-collapse: collapse; border:1px solid #000; text-align: center; width:325px;height:25px; padding:5px" class="text-center trn" colspan="2">Observed value III</td>
                    </tr>
					<tr>
                        <td style="border-collapse: collapse; border:1px solid #000; text-align: center; width:325px;height:25px; padding:5px" class="text-center trn" colspan="2">Measured</td>
                        <td style="border-collapse: collapse; border:1px solid #000; text-align: center; width:325px;height:25px; padding:5px" class="text-center" colspan="2" id="Nominal20">@Model.TorqueData.Nominal20</td>
                        <td style="border-collapse: collapse; border:1px solid #000; text-align: center; width:325px;height:25px; padding:5px" class="text-center" colspan="2" id="Nominal60">@Model.TorqueData.Nominal60</td>
                        <td style="border-collapse: collapse; border:1px solid #000; text-align: center; width:325px;height:25px; padding:5px" class="text-center" colspan="2" id="Nominal100">@Model.TorqueData.Nominal100</td>
                    </tr>
					<tr>
                        <td style="border-collapse: collapse; border:1px solid #000; text-align: center; width:325px;height:25px; padding:5px" class="text-center" colspan="8"></td>
                    </tr>
					<tr>
                        <td style="border-collapse: collapse; border:1px solid #000; text-align: center; width:325px;height:25px; padding:5px" class="text-center" colspan="2">1</td>
