@{
  ViewData["Title"] = "Home Page";
}

<!-- Main content   -->
<section class="content">
  <div class="container-fluid">
    <div class="card card-default">
      <div class="card-header">
        <h3 class="card-title">UserCreate</h3>

        <div class="card-tools">
          <button type="button" class="btn btn-tool" data-card-widget="collapse"><i class="fas fa-minus"></i></button>
          <button type="button" class="btn btn-tool" data-card-widget="remove"><i class="fas fa-remove"></i></button>
        </div>
      </div>
      <form>
        <div class="card-body">
          <div class="form-group">
            <label for="exampleInputUserId">UserId</label>
            <br>
            <input type="text" class="form-control-md-6" id="exampleInputUserId" placeholder="Enter email">
          </div>
          <div class="form-group">
            <label for="exampleInputUserName">UserName</label>
            <br>
            <input type="text" class="form-control-md-6" id="exampleInputUserName" placeholder="UserName">
          </div>
          <div class="form-group">
            <label for="exampleInputEmail">Email address</label>
            <br>
            <input type="email" class="form-control-md-6" id="exampleInputEmail" placeholder="Enter email">
          </div>
          <div class="form-group">
            <label for="exampleInputPassword1">Password</label>
            <br>
            <input type="password" class="form-control-md-6" id="exampleInputPassword1" placeholder="Password">
          </div>
          <div class="form-group">
            <label for="exampleInputRetypePassword">RetypePassword</label>
            <br>
            <input type="password" class="form-control-md-6" id="exampleInputRetypePassword"
              placeholder="Re-Type Password">
          </div>
          <div class="card-footer">
            <button type="submit" class="btn btn-primary">Submit</button>
          </div>
        </div>
      </form>
    </div>


    <div class="card card-default">
      <div class="card-header">
        <h3 class="card-title">GroupCreate</h3>

        <div class="card-tools">
          <button type="button" class="btn btn-tool" data-card-widget="collapse"><i class="fas fa-minus"></i></button>
          <button type="button" class="btn btn-tool" data-card-widget="remove"><i class="fas fa-remove"></i></button>
        </div>
      </div>

      <form>
        <div class="card-body">
          <div class="form-group">
            <label for="exampleInputGroupName">GroupName</label>
            <br>
            <input type="text" class="form-control-md-6" id="exampleInputGroupName" placeholder="Enter GroupName">
          </div>
          <div class="form-group">
            <label for="exampleInputEnterPerson">EnterPerson</label>
            <br>
            <input type="text" class="form-control-md-6" id="exampleInputEnterPerson" placeholder="EnterPerson">
          </div>
          <div class="form-group">
            <label for="exampleInputEntryDate">EntryDate</label>
            <br>
            <input type="text" class="form-control-md-6" id="exampleInputEntryDate" placeholder="EntryDate">
          </div>
          <div class="card-footer">
            <button type="submit" class="btn btn-primary">CreateGroup</button>
          </div>
        </div>
      </form>
    </div>
  </div>
</section>