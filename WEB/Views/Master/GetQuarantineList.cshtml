@{
    ViewData["Title"] = "Master Quarantine List";
}
    <section class="content">
        <div class="row">
            <div class="col-12">

                <div class="card card-primary"> 
                    <div class="card-header">
                        <div class="row">
                       <div class="col-md-6">
                            <h3 class="card-title trn">Master Quarantine List</h3>
                    </div>
                    <div class="col-md-6 text-right">
                        <a href='@Url.Action("Index","Master")'><input type="button" class="btn btn-sm btn-default trn" value="Master List" >                                
                            </a>
                    </div>
 
                       </div>  
                    </div>
                    <!-- /.card-header -->
                    <div class="card-body">
                        <table id="example1" class="table table-bordered table-striped">
                            <thead>
                                <tr>


                                <th class="trn">Master</th>
                                <th class="trn">Serial No</th>
                                <th class="trn">Lab ID No</th>
                                <th class="trn">Calibration Frequency</th>
                                <th class="trn">Calibration Date</th>
                                <th class="trn">Due Date</th>
                                <th class="trn">Quarantine Date</th>
                                <th class="trn">Quarantine Reason</th>
                                <th class="trn">Activate</th>
                            </thead>
                            <tbody>

                                @foreach (var item in Model)
                                {
                                    <tr id='row_@item.Id'>
                                        <td>@item.EquipName</td>
                                        <td>@item.SerialNo</td>
                                        <td>@item.LabId</td>
                                        <td>@item.CalibFreqId</td>
                                        <td>@item.CalibDate.ToString("dd-MM-yyyy")</td>
                                        <td>@item.DueDate.ToString("dd-MM-yyyy")</td>
                                        <td>@item.QuaraantineOn.ToString("dd-MM-yyyy")</td>
                                        <td>@item.QuarantineReason</td>
                                    <td><i class="fas fa-arrow-alt-circle-right" style="cursor:pointer;" id='@item.Id' onclick="MasterUnQuarantineClick(this, language)"></i></td>
                                    </tr>
                                }
                            </tbody>
                        </table>
                    </div>
                    <!-- /.card-body -->
                </div>
                <!-- /.card -->
            </div>
            <!-- /.col -->
        </div>
        <!-- /.row -->
    </section>
<script>
     setTimeout(function(){ 

      $("#example1").DataTable({
      "paging": true,
      "lengthChange": true,
      "searching": true,
      "ordering": true,
      "info": true,
      "autoWidth": true,
      "responsive": true,
    });
     }, 500);
    </script>