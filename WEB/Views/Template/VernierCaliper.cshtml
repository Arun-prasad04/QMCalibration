<table class="Tables-AndTableTextBox">
    <tr>
        <th colspan="2">For MUcalculation</th>
    </tr>
    <tr>
        <th>Interval</th>
        <td><input type="text" value="@Model.Interval" class="Tables-AndTableTextBox" id="MUcalculationInterval"></td>
    </tr>
    @foreach (var item in @Model.MuCalculationValues)
    {


        <tr>
            <td><input type="text" value="@item.MuLeftValue" class="Tables-AndTableTextBox" id="MuLeftValues"></td>
            <td><input type="text" value="@item.MuRightValue" class="Tables-AndTableTextBox" id="MuRightValues"></td>
        </tr>
    }
</table>
<br>
<table class="Tables-AndTableTextBox">
    <tr>
        <th>Calibration Performed By</th>
        <td><input type="text" value="@Model.CalibrationPerformedBy" class="Tables-AndTableTextBox"
                id="CalibrationPerformedBy"></td>
        <th>Reveiwed By</th>
        <td><input type="text" value="@Model.ReviewedBy" class="Tables-AndTableTextBox" id="ReveiwedBy"></td>
    </tr>
    <tr>
        <th>Date:</th>
        @if( true)
        {
            var performDate = @Model.CalibrationPerformedDate.ToString("dd-MM-yyyy");
            <td><input type="text" value="@performDate" id="CalibrationPerformedDate" name="CalibrationPerformedDate"
                    class="Tables-AndTableTextBox" disabled>
            </td>
        } 

        <th>Date:</th>
        <td><input type="text" value="@Model.ReviewedDate" class="Tables-AndTableTextBox" id="ReveiwedByDate"></td>

    </tr>
</table>