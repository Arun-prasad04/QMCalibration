<body class="hold-transition sidebar-mini layout-fixed">

	<style>
		.flag-icon-background {
			background-size: contain;
			background-position: 50%;
			background-repeat: no-repeat;
		}

		.flag-icon {
			background-size: cover;
			background-position: 50%;
			background-repeat: no-repeat;
			position: relative;
			display: inline-block;
			width: 1.33333333em;
			line-height: 1em;
		}

			.flag-icon:before {
				content: "\00a0";
			}

			.flag-icon.flag-icon-squared {
				width: 1em;
			}

		.flag-icon-jp {
			background-image: url(../image/jp.png);
		}

		.flag-icon-gb {
			background-image: url(../image/gb.png);
		}

		.Languagepicker .dropdown-menu > li > a:hover {
			color: #ffffff;
			text-decoration: none;
			background-color: #00677F;
		}

		.roledrop .dropdown-menu > li > a:hover {
			color: #ffffff;
			text-decoration: none;
			background-color: #00677F;
		}

		.roledrop {
			color: black;
			text-decoration: none;
			background-color: transparent;
			border: none;
			background: transparent;
			color: #000000;
			margin-top: 8pt;
			margin-right: 6pt;
			margin-left: 2pt;
		}

		.daimler_language {
			background: transparent;
			color: #000000;
			margin-top: 8pt;
			margin-right: 10pt;
			font-size: 12px;
		}

		.selected {
			background: #fff;
		}

		.layout-fixed .brand-link {
			width: 150px;
		}

		.dropdown-toggle {
			background: transparent;
			border: none;
		}

		.loader {
			border: 16px solid #f3f3f3;
			border-radius: 50%;
			border-top: 16px solid #3498db;
			/*width: 120px;
						height: 120px;*/
			display: block;
			position: fixed;
			top: 0;
			right: 0;
			bottom: 0;
			left: 0;
			margin: auto;
			z-index: 99;
			background-position: center;
			width: 105px;
			height: 105px;
			-webkit-animation: fa fa-spinner fa-spin fa-3x fa-fw;
			animation: a fa-spinner fa-spin fa-3x fa-fw;
			/*-webkit-animation: spin 2s linear infinite; /* Safari */
			/*animation: spin 2s linear infinite;*/
		}

		.overlay {
			position: fixed;
			display: none;
			/*width: 100%;
				  height: 100%;*/
			top: 0;
			left: 0;
			right: 0;
			bottom: 0;
			background-color: rgba(0,0,0,0.5);
			z-index: 2;
			cursor: pointer;
		}

		.overlays {
			/*/*background-color:darkgrey ; 	*/ */ position: absolute;
			top: 0;
			right: 0;
			bottom: 0;
			left: 0;
			opacity: 0.5;
			z-index: -1;
			backdrop-filter: blur(3px);
		}

		.muldrop {
			border: 2px solid #ced4da;
			border-radius: 3px;
			height: 33px !important;
			margin-bottom: 0.7rem !important;
			margin: 5px;
		}


		/* Safari */
		/*webkit-keyframes spin {
					  0% { -webkit-transform: rotate(0deg); }
					  100% { -webkit-transform: rotate(360deg); }
					}

					keyframes spin {
					  0% { transform: rotate(0deg); }
					  100% { transform: rotate(360deg); }
